Fun:Toggle("Vanish", "Spam Vanishes.", function(state)
	if state then
		_G.SpamVanish = true
		while _G.SpamVanish == true and wait(.5) do
			local args = {
				[1] = 0.2
				}
			game:GetService("Players").LocalPlayer.Backpack.ServerTraits.Vanish:FireServer(unpack(args))
		end
	else
		_G.SpamVanish = false
	end
end)
Fun:Toggle("Fake Ultra Instinct", "Funny Ultra Instinct.", function(state)
	if state then
		HRP["UltraTrans"]:Play()
		local form = Instance.new("Folder")
		form.Name = "ui"
		form.Parent = workspace.Live[plr.Name]
	else
		workspace.Live[plr.Name]:FindFirstChild("ui"):Destroy()
	end
end)
Fun:Toggle("Invisible**PATCHED**", "Turns you Invisible(Uses Flash Strike)", function(state)
	if state then
		if not plr.Backpack:FindFirstChild("Flash Strike") then
			Lib:Notifcation("Flash Strike not found!")
			Lib:InternalNotification("Bad","Flash Strike Not Found! \n Attempting to buy Flash Strike.")
			wait()
			game:GetService("ReplicatedStorage").AttemptBuy:InvokeServer({[1] = "Flash Strike"})
		end
		if plr.Backpack:FindFirstChild("Flash Strike") then
			plr.Backpack:FindFirstChild("Flash Strike").Parent = workspace.Live[plr.Name]
			if workspace.Live[plr.Name]["Flash Strike"].Activator:FindFirstChild("Animation") then
				workspace.Live[plr.Name]["Flash Strike"].Activator:FindFirstChild("Animation"):Destroy()
			end
			workspace.Live[plr.Name]["Flash Strike"]:Activate()
		end
	else
		if workspace.Live[plr.Name].Race.Value == "Saiyan" or workspace.Live[plr.Name].Race.Value == "Human" or workspace.Live[plr.Name].Race.Value == "Android" then
			local Tempshitvariablerenameme4
			local Ping = game:GetService("CoreGui").Holder.Main.HomeTab.UniversalProperties.PerfomancePropertiesTitles.PerfomanceProperties.PingPerfomance.Text:gsub("Ping: ", "")
			local proceedtime

			if tonumber(Ping) > 180 then
				proceedtime = .7
			else
				proceedtime = .32
			end
			local CurrentFrame4 = plr.Character.HumanoidRootPart.CFrame

			plr.Backpack.ServerTraits.ChatStart:FireServer(workspace.FriendlyNPCs["Hair Stylist"])
			wait(proceedtime)
			plr.Backpack.ServerTraits.ChatAdvance:FireServer({"Yes"})
			wait(proceedtime)
			plr.Backpack.HairScript.RemoteEvent:FireServer("woah")

			Tempshitvariablerenameme = plr.CharacterAdded:Connect(function(Character)
				local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
				repeat
					wait()
					HumanoidRootPart.CFrame = CurrentFrame1
				until Character:FindFirstChild("PowerOutput")
				Tempshitvariablerenameme:Disconnect()
				CurrentFrame1 = nil
			end)
			Lib:Notifcation("Respawned!")
		else
			workspace.Live[plr.Name].UpperTorso:Destroy()
		end
	end
end)
Fun:Seperator()	
