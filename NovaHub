Auto:Section("Auto-Farm")
if game.PlaceId ~= 535527772 or game.PlaceId ~= 3552158750 or game.PlaceId ~= 2050207304 then
	Auto:TextBox("NPC 1 ", "Auto Farm Option", "NPC 1 (Minimum)", NPCs, function(NPC1)
		VNPC1 = NPC1
	end)
	Auto:TextBox("NPC 2 ", "Auto Farm Option", "NPC 2 (Recommended)", NPCs, function(NPC2)
		VNPC2 = NPC2
	end)
	Auto:TextBox("NPC 3 ", "Auto Farm Option", "NPC 3 (Optional)", NPCs, function(NPC3)
		VNPC3 = NPC3
	end)
	Auto:TextBox("NPC 4 ", "Auto Farm Option", "NPC 4 (Optional)", NPCs, function(NPC4)
		VNPC4 = NPC4
	end)
	Auto:Toggle("Toggle Auto Farm", "Auto Farm Option", function(state)
		if state then
		-- Settings
		npcs = {
			VNPC1,
			VNPC2,
			VNPC3,
			VNPC4
		}
		-- Settings
		
		
		-- Services
		run = game:service('RunService')
		ts = game:service('TweenService')
		-- Services
		
		-- Var
		plr = game.Players.LocalPlayer
		char = plr.Character
		hrp = char:WaitForChild('HumanoidRootPart')
		live = workspace.Live
		ti = TweenInfo.new(30, Enum.EasingStyle.Circular, Enum.EasingDirection.Out)
		cumshot = Instance.new('BodyVelocity')
		mouse = plr:GetMouse()
		-- Var
		
		-- functions
		function afk()
			plr.Idled:Connect(function()
				game:service('VirtualUser'):MoveMouse(Vector2.new())
			end)
		end
		
		function noclip()
			for i,v in pairs(workspace:GetDescendants()) do
				if v:IsA('Part') then
					if v.CanCollide == true then
						v.CanCollide = false
					end
				end
			end
		end
		
		function godmode()
			if game.PlaceId == 536102540 then
				if workspace.Touchy.Part.Size ~= Vector3.new(100, 3, 100) then
					workspace.Touchy.Part.Size = Vector3.new(100, 3, 100)
				end
				if char:FindFirstChild('HumanoidRootPart') then
					workspace.Touchy.Part.CFrame = live[plr.Name].HumanoidRootPart.CFrame
				end
				if plr.PlayerGui:FindFirstChild('Popup') then
					plr.PlayerGui.Popup:Destroy()
				end
			end
		end
		
		function BV()
			cumshot.Name = 'cum'
			cumshot.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
			cumshot.P = 0
			cumshot.Velocity = Vector3.new(0,0,0)
			cumshot.Parent = workspace.Live[plr.Name].HumanoidRootPart
		end
		
		function Tween()
			if live[plr.Name].HumanoidRootPart.CFrame ~= CFrame.new(0,100,0) then
				ts:Create(live[plr.Name].HumanoidRootPart, TweenInfo.new(2, Enum.EasingStyle.Linear), {CFrame = CFrame.new(0,100,0)}):Play()
			end
		end
		
		function Disconnects()
			Demon = false
			AutoFarm:Disconnect()
			Tween:Pause()
			workspace.CurrentCamera.CameraType = Enum.CameraType.Custom
			live[plr.Name].HumanoidRootPart.cum:Destroy()
			workspace.Touchy.Part.CFrame = CFrame.new(-746.526306, 25.8025646, -6415.6543)
			workspace.Touchy.Part.Size = Vector3.new(20.45, 3.63, 2.54)
			for i,v in pairs(workspace:GetDescendants()) do
				if v:IsA('Part') then
					if v.CanCollide == false then
						v.CanCollide = true
					end
				end
			end
		end
		-- functions
		
		-- AutoFarm
		if live[plr.Name]:FindFirstChild("Animate") then
			live[plr.Name].Animate:Remove()
		end
		Tween()
		wait(2)
		afk()
		BV()
		noclip()
		
		AutoFarm = run.Heartbeat:connect(function()
			godmode()
			if Target:FindFirstChild('Humanoid') then
				if Target.Humanoid.Health > 0 then
					local distance = (live[plr.Name].HumanoidRootPart.Position - Target.HumanoidRootPart.Position).magnitude
					if distance <= 300 then
						live[plr.Name].HumanoidRootPart.CFrame = Target.HumanoidRootPart.CFrame + Target.HumanoidRootPart.CFrame.LookVector * -1.5
					end
					Tween = ts:Create(game.Players.LocalPlayer.Character.HumanoidRootPart, ti, {CFrame = Target.HumanoidRootPart.CFrame * CFrame.new(0,0,2)})
					Tween:Play()
					workspace.CurrentCamera.CameraType = Enum.CameraType.Track
					workspace.CurrentCamera.CFrame = live[plr.Name].HumanoidRootPart.CFrame * CFrame.new(1,1.5,10)
				end
			end
		end)
		
		Demon = true
		
		while Demon do
			wait()
			cumshot = #npcs
			names = {}
			for i, v in pairs(live:GetChildren()) do
				for cum = 1, cumshot do
					if string.match(string.lower(v.Name), string.lower(npcs[cum])) then
						table.insert(names, v)
						Target = v
						if v:FindFirstChild('Humanoid') then
							while v.Humanoid.Health > 0 and Demon do
								wait(0.33)
								plr.Backpack.ServerTraits.Input:FireServer({'md'}, 'InputObject', 'InputObject', false)
							end
						end
					end
				end
			end
		end
		else
			Disconnects()
		end
	end)
else
	Auto:Section("Unavaialable.")
end
