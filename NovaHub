	workspace.CurrentCamera.FieldOfView = f
end)
Fun:Button("Normal Fov", "Changes your FOV to the Default FOV", function()
	workspace.CurrentCamera.FieldOfView = 70
end)
Fun:Button("Transparent Body", "Makes your body Transparent(plr)", function()
	for i, v in pairs(workspace.Live[plr.Name]:GetChildren()) do
		if v:IsA("MeshPart") or v:IsA("Part") then
			v.Material = "ForceField"
			v.Color = Color3.fromRGB(255,255,255)
		end
	end
	Lib:Notifcation("Transformed.")
end)

