local function SaveInfo()
	Settings["1stLoaded"] = GotLoaded
    Settings["AutoBroly"] = AB
    Settings["AutoRejoin"] = Autorj
    Settings["HTCMoves"] = HTCMoves
    Settings["ABMoves"] = ABMoves
    Settings["SPAMMoves"] = SPAMMoves
    Settings["Admin"] = Admin
    Settings['Prefix'] = Keybnd
    writefile("Settings.lua", Http:JSONEncode(Settings))
end
