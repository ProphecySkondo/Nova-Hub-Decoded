Auto:Button("Begin Auto HTC", "Begins Auto HTC with the current settings", function()
	if game.PlaceId ~= 882375367 then
		Int:Teleport(882375367)
	end
	if game.PlaceId == 882375367 then
	repeat
		wait()
	until game.Players.LocalPlayer and game.Players.LocalPlayer.Character and game:IsLoaded()
		HRP.CFrame = CFrame.new(-59.8994, 237.834, -194.409)
		for i, v in pairs(plr.Backpack:GetChildren()) do
			if v.Name == "Dragon Throw" then
				v.Parent = game.Workspace.Live[plr.name]
				v:Activate()
				wait(0.3)
				v.Parent = Char["Dragon Throw"].Activator.Flip:Destroy()
				v:Activate()
			end
		end
		Run.Heartbeat:connect(function()
			HRP.CFrame = CFrame.new(-59.8994, 237.834, -194.409)
			plr.Backpack.ServerTraits.EatSenzu:FireServer(true)
		end)
		Run.Heartbeat:connect(function()
			if tonumber(plr.PlayerGui.HUD.Bottom.Stats.LVL.Val.Text) == 101 or tonumber(plr.PlayerGui.HUD.Bottom.Stats.LVL.Val.Text) == 181 or tonumber(plr.PlayerGui.HUD.Bottom.Stats.LVL.Val.Text) == 251 then 
				Int:Teleport(882375367)
			end
			for i, v in pairs(workspace.Live[plr.Name]:GetChildren()) do
				if v.Name == "Justice Combination" then
					local action = game.Players.LocalPlayer.Character:WaitForChild("Action")
					if action then
						wait()
						action:Destroy()
					end
				end
				if v.Name == "Action" or v.Name == "Attacking" or v.Name == "Using" or v.Name == "hyper" or v.Name == "Hyper" or v.Name == "KiBlasted" or v.Name == "Tele" or v.Name == "tele" or v.Name == "Killed"
				or v.Name == "Slow" or v.Name == "MoveStart" or v.Name == "KnockBacked" or v.Name == "Dodging" or v.Name == "zDASZzxc" or v.Name == "evasive" or v.Name == "BStun" or v.Name == "Ki Blasting"
				or v.Name == "NotHardBack" or v.Name == "Velocity" then
					v:Destroy()
				end
				if v.Name == "Block" and v.Value == true then
					v.Value = false
				end
			end
		end)
		Run.Stepped:connect(function()
            for _, Move in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
                if table.find(Moves, Move.Name) then
                    Move.Parent = workspace.Live[plr.Name]
                    Move:Activate()
                    wait(.3)
                    Move.Parent = game.Players.LocalPlayer.Backpack
                end
            end
        end)
	end
end)
