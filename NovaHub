if game.PlaceId == 536102540 then
	Buy:Section("Beans/Jars")
	Buy:DropDown('Type', 'Type Of Beans/Jars!', "Select a Type", {'Beans', 'Jars'}, function(option)
		if option == 'Beans' then
			Type = 'Beans'
		end
		if option == 'Jars' then
			Type = 'Jars'
		end
	end)
	Buy:DropDown("Amount", "Bean/Jar Amount", "Select an Amount", {"8", "80"}, function(o)
		if o == "8" then
			if Type == "Jars" then
				Lib:InternalNotification("Warning", "You Cannot Buy 8 Jars!")
			else
				Amount = "8"
			end
		end	
		if o == "80" then
			Amount = "80"
		end
	end)
		
	Buy:DropDown('Color', 'Color Of Beans/Jars', "Select a Color", {'Reds', 'Greens', 'Blues', 'Yellows'}, function(option)
		if option == 'Reds' then
			Color = 'Red'
		end
		if option == 'Blues' then
			Color = 'Blue'
		end
		if option == 'Greens' then
			Color = 'Green'
		end
		if option == 'Yellows' then
			Color = 'Yellow'
		end
	end)

	Buy:Toggle("Buy Beans","Begins Buying Beans!", function(state)
		if state then
			_G.BBuy = true 
			while _G.BBuy and wait() do
				plr.Backpack.ServerTraits.ChatStart:FireServer(workspace.FriendlyNPCs["Korin BEANS"])
				wait(.3)
				plr.Backpack.ServerTraits.ChatAdvance:FireServer({[1] = 'k'})
				wait(.3)
				plr.Backpack.ServerTraits.ChatAdvance:FireServer({[1] = 'k'})
				wait(.3)
				plr.Backpack.ServerTraits.ChatAdvance:FireServer({[1] = Type })
				wait(.3)
				plr.Backpack.ServerTraits.ChatAdvance:FireServer({[1] = 'k'})
				wait(.3)
				plr.Backpack.ServerTraits.ChatAdvance:FireServer({[1] = Amount})
				wait(.3)
				plr.Backpack.ServerTraits.ChatAdvance:FireServer({[1] = 'k'})
				wait(.3)
				plr.Backpack.ServerTraits.ChatAdvance:FireServer({[1] = Color})
				wait(.3)
				plr.Backpack.ServerTraits.ChatAdvance:FireServer({[1] = 'k'})
				wait(.3)
				if Amount == "8" then
					plr.Backpack.ServerTraits.ChatAdvance:FireServer({[1] = 'Use Zenni'})
				else
					plr.Backpack.ServerTraits.ChatAdvance:FireServer({[1] = 'Yes'})
				end
				wait(.3)
				plr.Backpack.ServerTraits.ChatAdvance:FireServer({[1] = 'k'})
				wait(1)
			end	
		else
			_G.BBuy = false
		end
	end)
	Buy:Toggle("Elder Kai", "Begins Auto Elder Kai!", function(state)
