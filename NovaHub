Visuals:Toggle("Purple Scythe", "Equips a Scythe on your back.", function(state)
	if state then   
		function m()
			s = Instance.new'MeshPart'
			s.Parent = workspace
			s.CFrame = Char.UpperTorso.CFrame * CFrame.new(0, 0, 1)
			s.Anchored = true
			s.MeshId = 'rbxassetid://9224065579'
			s.Material = Enum.Material.Neon
			s.Name = plr.Name .. 's'
			s.CanCollide = false
			s.Size = Vector3.new(3.7, 3.7, 3)
			s.Color = Color3.fromRGB(222, 101, 255)
		
			a = Instance.new'ParticleEmitter'
			a.Parent = workspace[plr.Name .. 's']
			a.Texture = 'rbxasset://textures/particles/sparkles_main.dds'
			a.Size = NumberSequence.new(.1)
			a.LockedToPart = true 	
			a.Rate = 20
			a.Rotation = NumberRange.new(0, 0)
			a.Lifetime = NumberRange.new(0.75, 0.75)
			a.RotSpeed = NumberRange.new(0, 0)
			a.SpreadAngle = Vector2.new(360, 360)
			a.ZOffset = 6
			a.LightEmission = 1
			a.Color = ColorSequence.new(Color3.fromRGB(255, 0, 255))
		
			attach = Instance.new('Attachment')
			attach.Name = "Poopy"
			attach.Parent = HRP
			attach.Position = Vector3.new(0, -3, 0)

			b = Instance.new'ParticleEmitter'
			b.Parent = attach
			b.LockedToPart = true
			b.Texture = 'rbxassetid://866471682'
			b.Rate = 50
			b.Rotation = NumberRange.new(180, 180)
			b.Lifetime = NumberRange.new(0.75, 0.75)
			b.RotSpeed = NumberRange.new(0, 0)
			b.Speed = NumberRange.new(13, 13)
			b.SpreadAngle = Vector2.new(360, 360)
			b.Acceleration = Vector3.new(0, 40, 0)
			b.ZOffset = 0
			b.LightEmission = 1
			b.Drag = 2.5
			b.Color = ColorSequence.new(Color3.fromRGB(255, 0, 255))
		end
		
		m()

		Run.Heartbeat:connect(function()
			s.Rotation = workspace.Live[plr.Name].UpperTorso.Rotation
			s.CFrame = workspace.Live[plr.Name].UpperTorso.CFrame * CFrame.new(.45, 0, .6)
		end)
		Lib:Notifcation("Equipped.")
	else
		HRP["Poopy"]:Destroy()
		workspace[plr.Name .. 's']:Destroy()
		Lib:Notifcation("Unequipped.")
	end
end)


Visuals:Toggle("Blue Scythe", "Equips a Scythe on your back.", function(state)
    if state then   
        function m()
            -- Create the scythe
            local s = Instance.new('MeshPart')
            s.Parent = workspace
            s.CFrame = Char.UpperTorso.CFrame * CFrame.new(0, 0, 1)
            s.Anchored = false -- Allow attachment
            s.MeshId = 'rbxassetid://9224065579'
            s.Material = Enum.Material.Neon
            s.Name = plr.Name .. 's'
            s.CanCollide = false
            s.Size = Vector3.new(3.7, 3.7, 3)
            s.Color = Color3.fromRGB(0, 102, 255) -- Blue Color
            
            -- Attach scythe to UpperTorso
            local weld = Instance.new("WeldConstraint")
            weld.Part0 = Char.UpperTorso
            weld.Part1 = s
            weld.Parent = s

            -- Add particle effect
            local a = Instance.new('ParticleEmitter')
            a.Parent = s
            a.Texture = 'rbxasset://textures/particles/sparkles_main.dds'
            a.Size = NumberSequence.new(.1)
            a.LockedToPart = true 	
            a.Rate = 20
            a.Rotation = NumberRange.new(0, 0)
            a.Lifetime = NumberRange.new(0.75, 0.75)
            a.RotSpeed = NumberRange.new(0, 0)
            a.SpreadAngle = Vector2.new(360, 360)
            a.ZOffset = 6
            a.LightEmission = 1
            a.Color = ColorSequence.new(Color3.fromRGB(0, 102, 255)) -- Blue particles

            -- Create attachment for trail particles
            local attach = Instance.new('Attachment')
            attach.Name = "Poopy"
            attach.Parent = HRP
            attach.Position = Vector3.new(0, -3, 0)

            -- Add trail particles
            local b = Instance.new('ParticleEmitter')
            b.Parent = attach
            b.LockedToPart = true
            b.Texture = 'rbxassetid://866471682'
            b.Rate = 50
            b.Rotation = NumberRange.new(180, 180)
            b.Lifetime = NumberRange.new(0.75, 0.75)
            b.RotSpeed = NumberRange.new(0, 0)
            b.Speed = NumberRange.new(13, 13)
            b.SpreadAngle = Vector2.new(360, 360)
            b.Acceleration = Vector3.new(0, 40, 0)
            b.ZOffset = 0
            b.LightEmission = 1
            b.Drag = 2.5
            b.Color = ColorSequence.new(Color3.fromRGB(0, 102, 255)) -- Blue trail particles
        end
        
        m()

        Lib:Notifcation("Equipped.")
    else
        -- Cleanup
        HRP:FindFirstChild("Poopy"):Destroy()
        local scythe = workspace:FindFirstChild(plr.Name .. 's')
        if scythe then scythe:Destroy() end
        Lib:Notifcation("Unequipped.")
    end
end)




-- Section: Animations
Visuals:Section("Animations")

-- Button to Check and Play Animation
Visuals:Button("Play Dragon Throw Animation", "You need to buy dragon throw", function()
    local player = game:GetService("Players").LocalPlayer
    local backpack = player:WaitForChild("Backpack")
    local itemName = "Dragon Throw" -- Name of the required item

    -- Check if the item exists in the backpack
    local item = backpack:FindFirstChild(itemName)

    if item and item:FindFirstChild("Activator") and item.Activator:FindFirstChild("Spinner") then
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:WaitForChild("Humanoid")
        local animator = humanoid:FindFirstChildOfClass("Animator") or humanoid:WaitForChild("Animator")
        local anim = item.Activator.Spinner

        -- Play the animation
        if anim:IsA("Animation") then
            local animationTrack = animator:LoadAnimation(anim)
            animationTrack:Play()
            Lib:Notification("Animation Played", "The Dragon Throw animation has started.")
        else
            Lib:Notification("Error", "Animation asset is invalid!")
        end
    else
        -- Notify the user to buy the missing item
        Lib:Notification("Buy Dragon Throw!", "Buy " .. itemName .. " to use this feature!")
    end
end)

local looping = false -- State to track looping

Visuals:Button("Fake Justice Stack", "You Have To Buy Kick Barrage", function()
    local player = game:GetService("Players").LocalPlayer
    local backpack = player:WaitForChild("Backpack")
    local itemName = "Kick Barrage" -- Required item name

    -- Check if the item exists in the backpack
    local item = backpack:FindFirstChild(itemName)

    if item and item:FindFirstChild("Activator") and item.Activator:FindFirstChild("Anim1") then
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoid = character:WaitForChild("Humanoid")
        local animator = humanoid:FindFirstChildOfClass("Animator") or humanoid:WaitForChild("Animator")
        local anim = item.Activator.Anim1

        -- Play the animation in a loop
        if anim:IsA("Animation") then
            looping = not looping -- Toggle looping state
            if looping then
                Lib:Notification("Started!", "Justice Stacking...")
                while looping and task.wait() do
                    local animationTrack = animator:LoadAnimation(anim)
                    animationTrack:Play()
                    animationTrack.Stopped:Wait() -- Wait for the animation to finish before restarting
                end
            else
                Lib:Notification("Looping Stopped", "The Kick Barrage animation has stopped.")
            end
        else
            Lib:Notification("Error", "Animation asset is invalid!")
        end
    else
        Lib:Notification("Buy Kick Barrage!")
    end
end)


