local User = Main:Tab("User", "User")
User:Section("Main")--WOOOOOOOOOOOW
User:Button("Xbox Chat", "Allows you to chat in Xbox and PC Chats", function()
	game:GetService("ReplicatedStorage").Xbox:FireServer()
	pcall(function()
		game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:Remove()
	end)
	plr.Chatted:Connect(function(message, recipient)
		pcall(function()
			args = {[1] = tostring(message)}
			game:GetService("ReplicatedStorage").Talk:FireServer(unpack(args))
		end)
	end)
	
	plr.CharacterAdded:Connect(function()
		pcall(function()
			game:GetService("ReplicatedStorage").Xbox:FireServer()
		end)
	end)
end)
User:DropDown("Power Meter", "Adjusts your power meter.", "Select Percentage", {"100", "95", "90", "85", "80", "75", "70", "65", "60", "55", "50", "45", "40", "35", "30", "25", "20", "15", "10", "5"}, function(o)
	local Options = {"100", "95", "90", "85", "80", "75", "70", "65", "60", "55", "50", "45", "40", "35", "30", "25", "20", "15", "10", "5"}
	if table.find(Options, o) then
		o = tonumber(o); CurrentPower = tonumber(CurrentPower)
		if CurrentPower ~= o then
			spawn(function()
				if CurrentPower < o then
					local Sub = o - CurrentPower
					local Div = Sub / 5
					for i = 1, Div do
						plr.Backpack.ServerTraits.Input:FireServer({"increase"}, true)
					end
					CurrentPower = CurrentPower + Sub
					Lib:Notification("Power Pecentage set to " .. o .."!")
				elseif CurrentPower > o then
					local Sub = CurrentPower - o
					local Div = Sub / 5
					for i = 1, Div do
						plr.Backpack.ServerTraits.Input:FireServer({"decrease"}, true)
					end
					CurrentPower = CurrentPower - Sub
					Lib:Notification("Power Pecentage set to " .. o .."!")
				end
			end)
		else
			Lib:Notification("Power Percentage Already " .. o .. "!")
		end
	end
end)
User:Button("Destroy fusion", "Destroys fusion Allowing you to play as if you're not fused.", function()
	if not Char:FindFirstChild("FUSED") then
		Lib:Notification("Not Fused!")
	end
	if Char:FindFirstChild("FUSED") then
		Char:FindFirstChild("FUSED"):Destroy()
		Lib:Notification("Fusion Destroyed!")
	end
end)
User:Slider("Speed", "Changes Walk/Run Speed.", 10, 1000, function(s)
	function setSpeed(walkspeedSet)
		local plr = game:GetService"Players".LocalPlayer
		local serverTraits = plr.Backpack:WaitForChild'ServerTraits'
	
		for i,v in next, getconnections(serverTraits.Input.OnClientEvent) do
			local speed = (s*(walkspeedSet/44))-s
			v:Fire({speed})
			break
		end
		pp = speed
	end
	setSpeed(s)
	
	Run.Hearbeat:connect(function()
		if Hum.WalkSpeed ~= s then
			setSpeed(s)
		end
	end)
end)
