    local Int = game:service'TeleportService'
    local Run = game:service'RunService'
    local ts = game:service'TweenService'
    
    local function BodyV()
        BodyVel = Instance.new("BodyVelocity")
        BodyVel.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
        BodyVel.Velocity = Vector3.new(0, 0, 0)
        BodyVel.Parent = HRP
    end
    local function Spam()
        for _, Move in next, plr.Backpack:GetChildren() do
            if table.find(Settings["ABMoves"], Move.Name) then
                Move.Parent = workspace.Live[plr.Name]
                Move:Activate()
                wait()
                Move.Parent = plr.Backpack
            end
        end
    end
    function noclip()
        for i,v in pairs(workspace:GetDescendants()) do
            if v:IsA('Part') then
                if v.CanCollide == true then
                    v.CanCollide = false
                end
            end
        end
    end
    local Place = {
        536102540,
        3565304751,
        2050207304,
    }
    
    if not table.find(Place, game.PlaceId) then
        if Queue == true then
            Int:Teleport(Place[2])
        else
            Int:Teleport(Place[1])
        end
    end
    
    if game.PlaceId == Place[1] then
        if Queue == true then
            Int:Teleport(Place[2])
        else
            wait(1)
            if not game.Workspace.Live:FindFirstChild(plr.Name) then
                ServerHop()
            end
