--Functions.
Gay = true
function respawn()
	Char:WaitForChild('HumanoidRootPart')
	Gay = false
	local CurrentFrame = workspace.Live[plr.Name].HumanoidRootPart
	
	plr.Backpack.ServerTraits.ChatStart:FireServer(workspace.FriendlyNPCs["Hair Stylist"])
	wait(0.3)
	plr.Backpack.ServerTraits.ChatAdvance:FireServer({"Yes"})
	wait(0.3)
	plr.Backpack.HairScript.RemoteEvent:FireServer("woah")
	Step = Run.Hearbeat:Connect(function()
		workspace.Live[plr.Name].HumanoidRootPart.CFrame = CurrentFrame.CFrame
	end)
	wait(.4)
	Step:Disconnect()
	Gay = true
end
CurrentPower = 100
