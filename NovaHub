    local function AddCmd(CommandName, CommandDesc, Type, Func)
        CMDS[#CMDS + 1] = {
            ["Name"] = CommandName,
            ["Description"] = CommandDesc,
            ["Event"] = Func,
        }
        local Img
        local Offset1
        local Offset2
        local CommandLabel = Instance.new("TextLabel")
        local public = Instance.new("ImageButton")

        if Type == "TP" or Type == "Teleport" or Type == "Teleportation" then
            Img = "rbxassetid://3926305904"
            Offset1 = Vector2.new(604, 324)
            Offset2 = Vector2.new(36, 36)
        elseif Type == "Tween" or Type == "Locate" or Type == "Location" then
            Img = "rbxassetid://3926307971"
            Offset1 = Vector2.new(884, 444)
            Offset2 = Vector2.new(36, 36)
        elseif Type == "Client" or Type == "User" or Type == "Player" then
            Img = "rbxassetid://3926307971"
            Offset1 = Vector2.new(884, 4)
            Offset2 = Vector2.new(36, 36)
        elseif Type == "List" then
            Img = "rbxassetid://3926307971"
            Offset1 = Vector2.new(404, 44)
            Offset2 = Vector2.new(36, 36)
        elseif Type == "Target" or Type == "Goal" or Type == "Bullseye" then
            Img = "rbxassetid://3926305904"
            Offset1 = Vector2.new(404, 764)
            Offset2 = Vector2.new(36, 36)
        elseif Type == "Auto" or Type == "Automatically" or Type == "Renew" or Type == "Redo" then
            Img = "rbxassetid://3926307971"
            Offset1 = Vector2.new(564, 444)
            Offset2 = Vector2.new(36, 36)
        end

        CommandLabel.Name = "CommandLabel"
        CommandLabel.Parent = CmdFrameScrollingFrame
        CommandLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        CommandLabel.BackgroundTransparency = 1.000
        CommandLabel.Position = UDim2.new(0.0500000045, 0, 0.193333328, 0)
        CommandLabel.Size = UDim2.new(0, 270, 0, 30)
        CommandLabel.Font = Enum.Font.Gotham
        CommandLabel.Text = CommandName.." - "..CommandDesc
        CommandLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        CommandLabel.TextSize = 12.000
        CommandLabel.TextXAlignment = Enum.TextXAlignment.Left

        public.Name = "public"
        public.Parent = CommandLabel
        public.BackgroundTransparency = 1.000
        public.Position = UDim2.new(0.907407403, 0, 0.0666666478, 0)
        public.Size = UDim2.new(0, 25, 0, 25)
        public.ZIndex = 2
        public.Image = Img
        public.ImageRectOffset = Offset1
        public.ImageRectSize = Offset2

        CmdFrameScrollingFrame.CanvasSize = UDim2.new(0,0,0,CmdFrameScrollingFrameListLayout.AbsoluteContentSize.Y)
    end

    -------------------------------------------------------------------------------- Functions.

    local function Notify(NotifMsg)
        local NotifyFrame = Instance.new("Frame")
        local NotifyTitle = Instance.new("TextLabel")
        local NotifyCorner = Instance.new("UICorner")
        local notifications = Instance.new("ImageButton")
        local shadowHolder = Instance.new("Frame")
        local umbraShadow = Instance.new("ImageLabel")
        local penumbraShadow = Instance.new("ImageLabel")
        local ambientShadow = Instance.new("ImageLabel")

        --Properties:

        NotifyFrame.Name = "Notify"
        NotifyFrame.Parent = CmdMain
        NotifyFrame.BackgroundColor3 = Color3.fromRGB(26, 26, 26)
        NotifyFrame.Position = UDim2.new(-1.975, 0, 15.4, 0)
        NotifyFrame.Size = UDim2.new(0, 265, 0, 50)

        NotifyTitle.Name = "NotifyTitle"
        NotifyTitle.Parent = NotifyFrame
        NotifyTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        NotifyTitle.BackgroundTransparency = 1.000
        NotifyTitle.Position = UDim2.new(0.0339622647, 0, 0.239999995, 0)
        NotifyTitle.Size = UDim2.new(0, 215, 0, 25)
        NotifyTitle.Font = Enum.Font.Gotham
        NotifyTitle.Text = NotifMsg
        NotifyTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
        NotifyTitle.TextSize = 14.000
        NotifyTitle.TextXAlignment = Enum.TextXAlignment.Left
        NotifyTitle.TextTransparency = 1.000

        NotifyCorner.CornerRadius = UDim.new(0, 4)
        NotifyCorner.Name = "NotifyCorner"
        NotifyCorner.Parent = NotifyFrame

        notifications.Name = "notifications"
        notifications.Parent = NotifyFrame
        notifications.BackgroundTransparency = 1.000
        notifications.Position = UDim2.new(0.869811296, 0, 0.24000001, 0)
        notifications.Size = UDim2.new(0, 25, 0, 25)
        notifications.ZIndex = 2
        notifications.Image = "rbxassetid://3926305904"
        notifications.ImageRectOffset = Vector2.new(844, 564)
        notifications.ImageRectSize = Vector2.new(36, 36)

        shadowHolder.Name = "shadowHolder"
        shadowHolder.Parent = NotifyFrame
        shadowHolder.BackgroundTransparency = 1.000
        shadowHolder.Size = UDim2.new(1, 0, 1, 0)

        umbraShadow.Name = "umbraShadow"
        umbraShadow.Parent = shadowHolder
        umbraShadow.AnchorPoint = Vector2.new(0.5, 0.5)
        umbraShadow.BackgroundTransparency = 1.000
        umbraShadow.Position = UDim2.new(0.5, 0, 0.5, 6)
        umbraShadow.Size = UDim2.new(1, 10, 1, 10)
        umbraShadow.Image = "rbxassetid://1316045217"
        umbraShadow.ImageColor3 = Color3.fromRGB(0, 0, 0)
        umbraShadow.ImageTransparency = 0.860
        umbraShadow.ScaleType = Enum.ScaleType.Slice
        umbraShadow.SliceCenter = Rect.new(10, 10, 118, 118)

        penumbraShadow.Name = "penumbraShadow"
        penumbraShadow.Parent = shadowHolder
        penumbraShadow.AnchorPoint = Vector2.new(0.5, 0.5)
        penumbraShadow.BackgroundTransparency = 1.000
        penumbraShadow.Position = UDim2.new(0.5, 0, 0.5, 6)
        penumbraShadow.Size = UDim2.new(1, 10, 1, 10)
        penumbraShadow.Image = "rbxassetid://1316045217"
        penumbraShadow.ImageColor3 = Color3.fromRGB(0, 0, 0)
        penumbraShadow.ImageTransparency = 0.880
        penumbraShadow.ScaleType = Enum.ScaleType.Slice
        penumbraShadow.SliceCenter = Rect.new(10, 10, 118, 118)

        ambientShadow.Name = "ambientShadow"
        ambientShadow.Parent = shadowHolder
        ambientShadow.AnchorPoint = Vector2.new(0.5, 0.5)
        ambientShadow.BackgroundTransparency = 1.000
        ambientShadow.Position = UDim2.new(0.5, 0, 0.5, 6)
        ambientShadow.Size = UDim2.new(1, 10, 1, 10)
        ambientShadow.Image = "rbxassetid://1316045217"
        ambientShadow.ImageColor3 = Color3.fromRGB(0, 0, 0)
        ambientShadow.ImageTransparency = 0.880
        ambientShadow.ScaleType = Enum.ScaleType.Slice
        ambientShadow.SliceCenter = Rect.new(10, 10, 118, 118)

        ts:create(NotifyFrame, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {Position = UDim2.new(-1.075, 0,15.4, 0)}):Play()
        ts:create(NotifyTitle, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
        wait(2)
        ts:create(NotifyTitle, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
        ts:create(NotifyFrame, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {Position = UDim2.new(-1.975, 0, 15.4, 0)}):Play()
        wait(.7)
        NotifyFrame:Destroy()
    end
    local function findCmd(name)
        for i = 1, #CMDS do
            local cmd = CMDS[i]
            if cmd.Name:lower() == name:lower() then
                return cmd
            end
        end
    end

    local function feedbackFound(name, desc)
        CmdTitle.Text = name
        CmdDesc.Text = desc
        CmdTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
        ts:create(CmdPreview, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {Position = UDim2.new(0.00333333318, 0, 1.2571429, 0)}):Play()
        ts:create(CmdPreview, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = .4}):Play()
        ts:create(CmdTitle, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
        ts:create(CmdDesc, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
    end

    local function feedbackError(string)
        if not ErrorDebounce then
            ErrorDebounce = true 
            CmdTitle.TextColor3 = Color3.fromRGB(235, 0, 0)
            CmdTitle.TextStrokeTransparency = 0
            CmdTitle.Font = Enum.Font.GothamBold
            CmdTitle.Text = string
            CmdDesc.Text = "Please try again."
            ts:create(CmdPreview, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {Position = UDim2.new(0.00333333318, 0, 1.2571429, 0)}):Play()
            ts:create(CmdPreview, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = .4}):Play()
            ts:create(CmdTitle, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
            ts:create(CmdDesc, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
            wait(2)
            ts:create(CmdPreview, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {Position = UDim2.new(0.003, 0,0, 0)}):Play()
            ts:create(CmdPreview, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
            ts:create(CmdTitle, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
            ts:create(CmdDesc, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
            CmdTitle.Font = Enum.Font.Gotham
            CmdTitle.TextColor3 = Color3.fromRGB(235, 0, 0)
            CmdTitle.TextStrokeTransparency = 1.000
            ErrorDebounce = false
        end
    end

    local function feedbackClose()
        repeat wait() until not ErrorDebounce
        ts:create(CmdPreview, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {Position = UDim2.new(0.003, 0,0, 0)}):Play()
        ts:create(CmdPreview, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
        ts:create(CmdTitle, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
        ts:create(CmdDesc, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
    end

