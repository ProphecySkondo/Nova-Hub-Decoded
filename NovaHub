    AddCmd("Reset", "Resets your character.", "Client", function()
        plr.Character:BreakJoints()
    end)
    AddCmd("Find", "Finds and redirects you the target.", "Client", function(Args)

        local Args = table.concat(Args, " ")

        local function findPlr(name)
            for i, v in pairs(game.Players:GetPlayers()) do
                if v.Name:lower():match(name:lower()) then
                    return v
                end
            end
        end

        local foundPlr = findPlr(Args)
        if not foundPlr then feedbackError("Failed to find '" .. Args .. "'.") return end
        ts:create(HRP, TweenInfo.new(1.2), {CFrame = foundPlr.Character.HumanoidRootPart.CFrame}):Play()
        Notify("Player Found - "..foundPlr.Name)
    end)
    AddCmd("NoSlow", "Removes Slow Values.", "Client", function()
        _G.NS = true
        local SteppyNigger = Run.RenderStepped:connect(function()
                if _G.NS == true then
                    for i,p in pairs(workspace.Live[plr.Name]:GetDescendants()) do
                        if p:IsA('BodyVelocity') and p.Name~="Flying" then
                            p:Destroy()
                        end
                    end
                    for i, v in pairs(Char:GetChildren()) do
                        if v.Name == "Action" or v.Name == "Attacking" or v.Name == "Using" or v.Name == "hyper" or v.Name == "Hyper" or v.Name == "KiBlasted" or v.Name == "Tele" or v.Name == "tele" or v.Name == "Killed"
                        or v.Name == "Slow" or v.Name == "MoveStart" or v.Name == "KnockBacked" or v.Name == "Dodging" or v.Name == "zDASZzxc" or v.Name == "evasive" or v.Name == "BStun" or v.Name == "Ki Blasting"
                        or v.Name == "NotHardBack" or v.Name == "Velocity" then
                        v:Destroy()
                    end
                    if v.Name == "Block" and v.Value == true then
                        v.Value = false
                    end
                end
            end
        end)
    end)
	AddCmd("Slow", "Disables No Slow.", "Client", function()
		_G.NS = false
	end)


    AddCmd("LowGFX", "Low GFX For Your Game", "Auto", function()

        local decalsyeeted = true
        local g = game
        local w = g.Workspace
        local l = g.Lighting
        local t = w.Terrain
        t.WaterWaveSize = 0
        t.WaterWaveSpeed = 0
        t.WaterReflectance = 0
        t.WaterTransparency = 0
        l.GlobalShadows = false
        l.FogEnd = 9e9
        l.Brightness = 0
        settings().Rendering.QualityLevel = "Level01"
        for i, v in pairs(g:GetDescendants()) do
            if v:IsA("Part") or v:IsA("Union") or v:IsA("CornerWedgePart") or v:IsA("TrussPart") then
                v.Material = "Plastic"
                v.Reflectance = 0
            elseif v:IsA("Decal") or v:IsA("Texture") and decalsyeeted then
                v.Transparency = 1
            elseif v:IsA("ParticleEmitter") or v:IsA("Trail") then
                v.Lifetime = NumberRange.new(0)
            elseif v:IsA("Explosion") then
                v.BlastPressure = 1
                v.BlastRadius = 1
            elseif v:IsA("Fire") or v:IsA("SpotLight") or v:IsA("Smoke") or v:IsA("Sparkles") then
                v.Enabled = false
            elseif v:IsA("MeshPart") then
                v.Material = "Plastic"
                v.Reflectance = 0
                v.TextureID = 10385902758728957
            end
        end
        for i, e in pairs(l:GetChildren()) do
            if e:IsA("BlurEffect") or e:IsA("SunRaysEffect") or e:IsA("ColorCorrectionEffect") or e:IsA("BloomEffect") or e:IsA("DepthOfFieldEffect") then
                e.Enabled = false
            end
        end

        Lib:Notification("Low GFX On", "Low GFX Is On Rejoin To Romove")
    end)


    AddCmd("GodMode", "Enables GodMode for your character.", "Auto", function()
        if not godModeEnabled then
            godModeEnabled = true
            game:GetService("RunService").RenderStepped:connect(function()
                if godModeEnabled then
                    -- Fire the events to prevent interaction with the part (immunity)
                    firetouchinterest(game:GetService("Workspace").Touchy.Part, game.Players.LocalPlayer.Character.HumanoidRootPart, 0)
                    firetouchinterest(game:GetService("Workspace").Touchy.Part, game.Players.LocalPlayer.Character.HumanoidRootPart, 1)
                    -- Disable any popups
                    game:GetService("Players").LocalPlayer.PlayerGui.Popup.Enabled = false
                end
            end)
            Lib:Notification("GodMode Enabled", "Your character is now in GodMode.")
        else
            Lib:Notification("GodMode Already Enabled", "GodMode is already active.")
        end
    end)

    AddCmd("GodModeOff", "Disables GodMode for your character.", "Auto", function()
        if godModeEnabled then
            godModeEnabled = false
            Lib:Notification("GodMode Disabled", "Your character is no longer in GodMode.")
        else
            Lib:Notification("GodMode Not Active", "GodMode is already disabled.")
        end
    end)

    AddCmd("Anchor", "Toggles anchoring for your character.", "Auto", function()
        -- Anchor the character when the command "Anchor" is used
        local player = game:GetService("Players").LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
        
        -- Anchor the character
        humanoidRootPart.Anchored = true
        Lib:Notification("Anchored", "Your character has been anchored.")
    end)
    
    AddCmd("AnchorOff", "Disables anchoring for your character.", "Auto", function()
        local player = game:GetService("Players").LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()
        local humanoidRootPart = character:WaitForChild("HumanoidRootPart")
        
        humanoidRootPart.Anchored = false
        Lib:Notification("Unanchored", "Your character has been unanchored.")
    end)
    
    AddCmd("Fling All", "Flings All", "Auto", function()


    loadstring(game:HttpGet("https://pastebin.com/raw/zqyDSUWX"))()


    end)

    AddCmd("KiSpam", "Spams Ki Moves.", "Auto", function()
        _G.KS = true
        while _G.KS and wait() do
            for i,v in pairs(plr.Backpack:GetChildren()) do
                if v.Name == "Super Volley" or v.Name == "Unrelenting Volley" or v.Name == "Sudden Storm"  or v.Name == "Chain Destructo Disk" or v.Name == "Blaster Meteor" then
                    v.Parent = Char
                    wait()
                    v:Activate()
                    v:Deactivate()
                    v.Parent = plr.Backpack
                end
            end
        end
    end)
    AddCmd("MeleeSpam", "Spams Melee Moves.", "Auto", function()
        _G.MS = true
        while _G.MS and wait() do
            for i,v in pairs(plr.Backpack:GetChildren()) do
                if v.Name == "Anger Rush" or v.Name == "TS Molotov" or v.Name == "Flash Skewer" or v.Name == "Neo Wolf Fang Fist"  or v.Name == "Sweep Kick" or v.Name == "Meteor Crash" or v.Name == "Wolf Fang Fist" then
                    v.Parent = Char
                    wait()
                    v:Activate()
                    v:Deactivate()
                    v.Parent = plr.Backpack
                end
            end
        end
    end)
    AddCmd("Unload", "Unloads Nova Command bar.", "List", function()
        if _G.MS == true then
            _G.MS = false
        elseif _G.KS == true then
            _G.KS = false
        elseif _G.NS == true then
            _G.NS = false
        end
        Holder:Destroy()
    end)
    AddCmd("DisableAdmin", "Disables Admin", "List", function()
        Admin = false
        SaveInfo()
        Notify("Relaunch Nova.")
    end)

    if game.PlaceId == Place[1] then
        AddCmd("respawn", "respawns you.", "Client", function()
            currentframe = workspace.Live[plr.Name].HumanoidRootPart
            local Step = game:service'RunService'.Heartbeat:Connect(function()
                HRP.CFrame = currentframe.CFrame
                if workspace.FriendlyNPCs:FindFirstChild'Hair Stylist' then
                    plr.Backpack.ServerTraits.ChatStart:FireServer(workspace.FriendlyNPCs["Hair Stylist"])
                    wait()
                    plr.Backpack.ServerTraits.ChatAdvance:FireServer({"Yes"})
                    repeat
                        wait()
                    until
                    plr.Backpack.HairScript.RemoteEvent:FireServer("woah")
                end
            end)
            local Step2 = game:service'RunService'.Heartbeat:Connect(function()
                HRP.CFrame = currentframe.CFrame
            end)
            wait(.9)
            Step:Disconnect()
            wait(1.2)
            Step2:Disconnect()
        end)
        AddCmd("South", "Tweens you to South City.", "Location", function()
            ts:create(HRP, TweenInfo.new(1.2), {CFrame = CFrame.new(-457, 28, -6412)}):Play()
        end)
        AddCmd("West", "Tweens you to West City.", "Location", function()
            ts:create(HRP, TweenInfo.new(1.2), {CFrame = CFrame.new(-571, 23, -2885)}):Play()
        end)
        AddCmd("Central", "Tweens you to Central City.", "Location", function()
            ts:create(HRP, TweenInfo.new(1.2), {CFrame = CFrame.new(-3835, 23, -1428)}):Play()
        end)
        AddCmd("East", "Tweens you to East City.", "Location", function()
            ts:create(HRP, TweenInfo.new(1.2), {CFrame = CFrame.new(-6541, 22, -918)}):Play()
        end)
        AddCmd("Satan", "Tweens you to Satan City.", "Location", function()
            ts:create(HRP, TweenInfo.new(1.2), {CFrame = CFrame.new(-5781, 140, -3043)}):Play()
        end)
        AddCmd("Parsley", "Tweens you to Parsley City.", "Location", function()
            ts:create(HRP, TweenInfo.new(1.2), {CFrame = CFrame.new(-1979, 22, -3152)}):Play()
        end)
        AddCmd("Korin", "Tweens you to Korin Tower.", "Location", function()
            ts:create(HRP, TweenInfo.new(1.2), {CFrame = CFrame.new(2071, 1496, -2279)}):Play()
        end)
        AddCmd("Lookout", "Tweens you to The Lookout.", "Location", function()
            ts:create(HRP, TweenInfo.new(1.2), {CFrame = CFrame.new(2470, 3945, -2275)}):Play()
        end)
    end

    -------------------------------------------------------------------------------- Interactions.

    mouse.KeyDown:connect(function(key)
        if key == Settings["Prefix"] and not CmdTextBox:IsFocused() then
            ts:create(CmdMain, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = 0}):Play()
            ts:create(umbraShadow, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {ImageTransparency = 0.860}):Play()
            ts:create(penumbraShadow, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {ImageTransparency = 0.880}):Play()
            ts:create(ambientShadow, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {ImageTransparency = 0.880}):Play()
            ts:create(CmdTextBox, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
            Popup:Play()
            CmdTextBox.Visible = true
            CmdTextBox:CaptureFocus()
            task.wait()
            CmdTextBox.Text = ""
        end
    end)

    CmdTextBox.FocusLost:connect(function(EnterPressed)
		if EnterPressed then
	
			local String = CmdTextBox.Text 
			local fullArgs = String:split(" ") 
			local Command = findCmd(table.remove(fullArgs, 1)) 
	
			if not Command then
				CmdTextBox.Text = ""
				feedbackError("Unknown command '" .. String .. "'")

				wait(.5)
				ts:create(CmdMain, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
				ts:create(CmdTextBox, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
				ts:create(EnterLabel, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
				ts:create(create, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {ImageTransparency = 1}):Play()
				ts:create(umbraShadow, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {ImageTransparency = 1}):Play()
				ts:create(penumbraShadow, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {ImageTransparency = 1}):Play()
				ts:create(ambientShadow, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {ImageTransparency = 1}):Play()
			else
				Entered:Play()
				Command.Event(fullArgs)
				feedbackClose()
				CmdTextBox.Text = ""
				feedbackClose()

				wait(.5)
				ts:create(CmdMain, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
				ts:create(CmdTextBox, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
				ts:create(EnterLabel, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
				ts:create(create, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {ImageTransparency = 1}):Play()
				ts:create(umbraShadow, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {ImageTransparency = 1}):Play()
				ts:create(penumbraShadow, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {ImageTransparency = 1}):Play()
				ts:create(ambientShadow, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {ImageTransparency = 1}):Play()
			end
		else
			feedbackClose()
			ts:create(CmdMain, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
			ts:create(CmdTextBox, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
			ts:create(EnterLabel, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
			ts:create(create, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {ImageTransparency = 1}):Play()
			ts:create(umbraShadow, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {ImageTransparency = 1}):Play()
			ts:create(penumbraShadow, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {ImageTransparency = 1}):Play()
			ts:create(ambientShadow, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {ImageTransparency = 1}):Play()
		end
	end)

    CmdTextBox:GetPropertyChangedSignal("Text"):connect(function()
        if CmdTextBox.Text:len() > 0 then
            local Command = findCmd(CmdTextBox.Text)
            if Command then
                ts:create(EnterLabel, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
                feedbackFound(Command.Name, Command.Description)
            else
                feedbackClose()
                ts:create(EnterLabel, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
            end
            ts:create(create, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {ImageTransparency = 0}):Play()
        else
            feedbackClose()
            ts:create(EnterLabel, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
            ts:create(create, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {ImageTransparency = 1}):Play()
            ts:create(BackText, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
        end
    end)
end
plr.CharacterAdded:Connect(function(Char)
	Char = Char
	HRP = Char:WaitForChild"HumanoidRootPart"
	Hum = Char:WaitForChild"Humanoid"
end)
Lib:Notification("Loaded.")

-- Screen Blur Setup
local lighting = game:GetService("Lighting")
local blurEffect = Instance.new("BlurEffect")
blurEffect.Parent = lighting
blurEffect.Size = 10  -- Adjust the blur size (higher = stronger blur)
blurEffect.Enabled = false  -- Initially disabled

-- Function to toggle screen blur based on Nova visibility
local function toggleBlur(state)
    blurEffect.Enabled = state
end

-- Listen for F1 key press to toggle visibility of Nova and screen blur
game:GetService("UserInputService").InputBegan:connect(function(a, b)
    if a.KeyCode == Enum.KeyCode.F1 and not b then
        -- Toggle the visibility of Nova (or your GUI element)
        local novaVisible = not game.CoreGui.Holder.Main.Visible
        game.CoreGui.Holder.Main.Visible = novaVisible

        -- Enable or disable screen blur based on Nova visibility
        toggleBlur(novaVisible)

        -- Handle notification based on Nova visibility
        if novaVisible then
            Lib:Notification("Nova Visible", "Nova Blur and screen blur are now active.")
        else
            Lib:Notification("Nova Hidden", "Nova Blur and screen blur are now inactive.")
        end
    end
end)
