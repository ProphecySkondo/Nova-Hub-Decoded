	User:Toggle("Anti Glitch", "Prevents you from getting glitched!", function(state)
		if state then
			local Tempshitvariablerenameme3
			local Ping = game:GetService("CoreGui").Holder.Main.HomeTab.UniversalProperties.PerfomancePropertiesTitles.PerfomanceProperties.PingPerfomance.Text:gsub("Ping: ", "")
			local proceedtime

			if tonumber(Ping) > 180 then
				proceedtime = .7
			else
				proceedtime = .32
			end
			_G.DontGlitchMe = true
			
			local char = plr.Character


			function AntiGlitch()
				char.DescendantAdded:connect(function(v)
					game:GetService("RunService").Stepped:wait()
					if (not v:FindFirstAncestorWhichIsA("Tool") and 
						v:IsA("BodyVelocity") and v.Name~="Flying") then
						v:Destroy()
					end
					local Hum = char:FindFirstChildOfClass("Humanoid") or char:FindFirstChildOfClass("AnimationController")
						
					for i,v in next, Hum:GetPlayingAnimationTracks() do
						v:Stop()
					end
				end)
			end

			Run.Heartbeat:connect(function()
				if _G.DontGlitchMe == true then
    				for i, v in pairs(workspace.Live[plr.Name]:GetChildren()) do
						if v.Name == "MoveStart" then
    						AntiGlitch()
							
							local CurrentFrame3 = plr.Character.HumanoidRootPart.CFrame
							plr.Backpack.ServerTraits.ChatStart:FireServer(workspace.FriendlyNPCs["Hair Stylist"])
							wait(proceedtime)
							plr.Backpack.ServerTraits.ChatAdvance:FireServer({"Yes"})
							wait(proceedtime)
