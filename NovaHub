        HRP.CFrame = Broly.Head.CFrame * CFrame.new(0, -2, 2)
        workspace.CurrentCamera.CFrame = Broly.Head.CFrame
        Hum:EquipTool(plr.Backpack["Dragon Throw"])
        wait(.5)
        Run.Stepped:connect(function()
            if Char:FindFirstChild('Dragon Throw') then
                Char["Dragon Throw"]:Activate()
                if Char["Dragon Throw"].Activator:FindFirstChild("Flip") then
                    Char["Dragon Throw"].Activator.Flip:Destroy()
                end
            end
        end)
        wait(1.3)
        if not Broly:FindFirstChild('MoveStart') then
            if Queue == false then
                Int:Teleport(World[1])
            else
                Int:Teleport(World[2])
            end
        end
        Run.Heartbeat:connect(function()
            for i, v in pairs(Char:GetChildren()) do
                if v.Name == "Action" or v.Name == "Attacking" then
                    v:Destroy()
                end
            end
        end)
        wait(.2)
        Run.Heartbeat:connect(function()
            HRP.CFrame = CFrame.new(-19, -127, -16)
            Spam()
            plr.Backpack.ServerTraits.EatSenzu:FireServer(true)
            if Char.Ki.Value / Char.Ki.MaxValue * 100 <= 5 then
                plr.Backpack.ServerTraits.Input:FireServer({[1] = "m2"}, CFrame.new(0, 0, 0), nil, false)
            end
        end)
        Run.Heartbeat:connect(function()
            if Char.Ki.Value / Char.Ki.MaxValue * 100 <= 5 then
                if Char.Race.Value == "Android" then
                    plr.Backpack.ServerTraits.Transform:FireServer('g')
                end
                if Char.Race.Value ~= "Android" then
                    plr.Backpack.ServerTraits.Transform:FireServer('h')
                end
            end
        end)
        Run.Heartbeat:connect(function()
            if Char.Humanoid.Health <= Char.Humanoid.MaxHealth * 0.2 then
                plr.Backpack.ServerTraits.Use:InvokeServer(game.Workspace.Live[plr.name]:FindFirstChild('Ki').Value)
                plr.Backpack.ServerTraits.Transform:FireServer('g')
            end
        end)
        Run.Heartbeat:connect(function()
            if Broly.Humanoid.Health == 0 then
                if Queue == true then
                    wait(.2)
