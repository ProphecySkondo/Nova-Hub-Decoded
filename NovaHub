Other:Section("Utilities")
Other:Button("Rejoin", "Rejoins The Current Instance.", function()


	local TeleportService = game:GetService("TeleportService")
local Players = game:GetService("Players")

local function rejoin()
    local player = Players.LocalPlayer
    if not player then return end -- Ensures there's a player instance.

    if #Players:GetPlayers() <= 1 then
        player:Kick("\nRejoining.")
        wait(0.5) -- Small delay to ensure the kick message shows.
        TeleportService:Teleport(game.PlaceId, player)
    else
        TeleportService:TeleportToPlaceInstance(game.PlaceId, game.JobId, player)
    end
end

-- Call the rejoin function or bind it to a command system if necessary.
rejoin()



end)
