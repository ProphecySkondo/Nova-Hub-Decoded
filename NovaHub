            HRP.CFrame = CFrame.new(3440, 238, 172)
            wait(.4)
            Char.LowerTorso:BreakJoints()
            local Numbers  = {1, 2, 3, 4, 5, 6, 7}
            local RandomNumber = math.random(1, #Numbers)
            
            Run.Heartbeat:connect(function()
                if RandomNumber == 1 then
                    HRP.CFrame = CFrame.new(-27, 238, -143)
                elseif RandomNumber == 2 then
                    HRP.CFrame = CFrame.new(-27, 238, -286)
                elseif RandomNumber == 3 then
                    HRP.CFrame = CFrame.new(-28, 238, -420)
                elseif RandomNumber == 4 then
                    HRP.CFrame = CFrame.new(-26, 238, -590)
                elseif RandomNumber == 5 then
                    HRP.CFrame = CFrame.new(-27, 238, -731)
                elseif RandomNumber == 6 then
                    HRP.CFrame = CFrame.new(-27, 238, -881)
                elseif RandomNumber == 7 then
                    HRP.CFrame = CFrame.new(-27, 238, -1048)
                end
            end)
            wait(25)
            Int:Teleport(Place[2])
        end
    end
    
    if game.PlaceId == Place[3] then
        local Broly = game.Workspace.Live:WaitForChild('Broly BR')
        BodyV()
        noclip()
