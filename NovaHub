								local HumanoidRootPart = Character:WaitForChild("HumanoidRootPart")
								repeat
									wait()
									HumanoidRootPart.CFrame = CurrentFrame3
								until Character:FindFirstChild("PowerOutput")
								Tempshitvariablerenameme:Disconnect()
								CurrentFrame3 = nil
								Lib:Notification("Respawned!")
							end)
    					end
    				end
				end
			end)
		else
			_G.DontGlitchMe = false
		end
	end)
end
if game.PlaceId == 535527772 then
	User:Button("Respawn", "Respawns you In TOP", function()
		if not workspace.Live[plr.Name]:FindFirstChild("SuperAction") then
			Lib:Notifcation("You're Not Dead.")
		end
		if workspace.Live[plr.Name]:FindFirstChild("SuperAction") then
			workspace.Live[plr.Name]:FindFirstChild("SuperAction"):Destroy()
			ts:create(workspace.Live[plr.Name].HumanoidRootPart, TweenInfo.new(.3), {CFrame = CFrame.new(100, 100, 100)}):Play()
			wait(.5)
			Lib:Notifcation("Respawned!")
		end
	end)
end
User:Toggle("No Slow", "Removes Slow Values.", function(state)
	if state then
		NS = false
		if NS == false then
			NS = true
			SteppyNigger = Run.RenderStepped:connect(function()
				if NS == true then
					for i, p in pairs(workspace.Live[plr.Name]:GetDescendants()) do
						if p:IsA("BodyVelocity") and p.Name~= "Flying" then
							p:Destroy()
						end
					end
					for i, v in pairs(workspace.Live[plr.Name]:GetChildren()) do
						if v.Name == "Justice Combination" then
							local action = Char:WaitForChild("Action")
							if action then
								wait()
							action:Destroy()
						end
					end
					if v.Name == "Action" or v.Name == "Attacking" or v.Name == "Using" or v.Name == "hyper" or v.Name == "Hyper" or v.Name == "KiBlasted" or v.Name == "Tele" or v.Name == "tele" or v.Name == "Killed"
					or v.Name == "Slow" or v.Name == "MoveStart" or v.Name == "KnockBacked" or v.Name == "Dodging" or v.Name == "zDASZzxc" or v.Name == "evasive" or v.Name == "BStun" or v.Name == "Ki Blasting"
					or v.Name == "NotHardBack" or v.Name == "Velocity" then
						v:Destroy()
					end
					if v.Name == "Block" and v.Value == true then
						v.Value = false
					end
				end
				end
			end)
		end
	elseif NS == true then
		NS = false
		SteppyNigger:Disconnect()
	end
end)
User:Toggle("Use Bean/Jar(s)", "Uses Bean/Jar(s) Every 30 Secs.", function(state)
	if state then
		_G.BS = true
		while _G.BS and wait() do
			plr.Backpack.ServerTraits.EatSenzu:FireServer(true)
		end
	else
		_G.BS = false
	end
end)


User:Section("Extras")

User:Toggle("Anchored", "Toggles player anchoring", function(state)
    if state then
        plr.Character.HumanoidRootPart.Anchored = true
        Lib:Notification("Anchored Enabled", "Your character is now anchored!")
    else
        plr.Character.HumanoidRootPart.Anchored = false
        Lib:Notification("Anchored Disabled", "Your character is now unanchored!")
    end
end)


