            ToggleCircle.ScaleType = Enum.ScaleType.Slice
            ToggleCircle.SliceCenter = Rect.new(100, 100, 100, 100)
            ToggleCircle.SliceScale = 0.120

            shuffle.Name = "shuffle"
            shuffle.Parent = Toggle
            shuffle.BackgroundTransparency = 1.000
            shuffle.LayoutOrder = 7
            shuffle.Position = UDim2.new(0.935000002, 0, 0.283333331, 0)
            shuffle.Size = UDim2.new(0, 25, 0, 25)
            shuffle.ZIndex = 2
            shuffle.Image = "rbxassetid://3926307971"
            shuffle.ImageRectOffset = Vector2.new(164, 44)
            shuffle.ImageRectSize = Vector2.new(36, 36)

            Toggle.MouseEnter:Connect(function()
                ts:Create(Toggle, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 0}):Play()
            end)
            Toggle.MouseLeave:Connect(function()
                ts:Create(Toggle, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 1}):Play()
            end)
                
            ToggleButton.MouseButton1Click:Connect(function()
                if toggled == false then
                    ts:Create(ToggleFrame, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {ImageColor3 = Color3.fromRGB(0, 255, 0)}):Play()
                    ToggleCircle:TweenPosition(UDim2.new(0.677, -5,0.087, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Quart, .3, true)
                    wait(.1)
                else
                    ts:Create(ToggleFrame, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {ImageColor3 = Color3.fromRGB(79, 5, 111)}):Play()
                    ToggleCircle:TweenPosition(UDim2.new(0.2, -5,0.087, 0), Enum.EasingDirection.Out, Enum.EasingStyle.Quart, .3, true)
                end
                toggled = not toggled
                pcall(callback, toggled)
            end)

            CurrentTab.CanvasSize = UDim2.new(0,0,0,List.AbsoluteContentSize.Y)
        end
        function TabContent:TextBox(TxtTitle, TxtDesc, Overlay, Indextable, callback)
            local Index = Indextable or {}
            local Display = Overlay or "..."
            local callback = callback or function() end
            local TextBox = Instance.new("Frame")
            local TextBoxTitle = Instance.new("TextLabel")
            local TextBoxDesc = Instance.new("TextLabel")
            local TextBoxCorner = Instance.new("UICorner")
            local WriteBox = Instance.new("TextBox")
            local WriteBoxUnderFrame = Instance.new("Frame")
            local text_format = Instance.new("ImageButton")

            --Properties:

            TextBox.Name = TxtTitle.."TextBox"
            TextBox.Parent = CurrentTab
            TextBox.BackgroundTransparency = 1.000
            TextBox.BackgroundColor3 = Color3.fromRGB(26, 26, 26)
            TextBox.Position = UDim2.new(0.0161290318, 0, 0.188888893, 0)
            TextBox.Size = UDim2.new(0, 600, 0, 60)

            TextBoxTitle.Name = TxtTitle.."TextBoxTitle"
            TextBoxTitle.Parent = TextBox
            TextBoxTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            TextBoxTitle.BackgroundTransparency = 1.000
            TextBoxTitle.Position = UDim2.new(0.0333333313, 0, 0.106666565, 0)
            TextBoxTitle.Size = UDim2.new(0, 196, 0, 25)
            TextBoxTitle.Font = Enum.Font.Gotham
            TextBoxTitle.Text = TxtTitle
            TextBoxTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
            TextBoxTitle.TextSize = 14.000
            TextBoxTitle.TextXAlignment = Enum.TextXAlignment.Left

            TextBoxDesc.Name = TxtTitle.."TextBoxDesc"
            TextBoxDesc.Parent = TextBoxTitle
            TextBoxDesc.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            TextBoxDesc.BackgroundTransparency = 1.000
            TextBoxDesc.Position = UDim2.new(0, 0, 0.746666551, 0)
            TextBoxDesc.Size = UDim2.new(0, 196, 0, 25)
            TextBoxDesc.Font = Enum.Font.Gotham
            TextBoxDesc.Text = TxtDesc
            TextBoxDesc.TextColor3 = Color3.fromRGB(255, 255, 255)
            TextBoxDesc.TextSize = 12.000
            TextBoxDesc.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
            TextBoxDesc.TextXAlignment = Enum.TextXAlignment.Left

            TextBoxCorner.CornerRadius = UDim.new(0, 4)
            TextBoxCorner.Name = "TextBoxCorner"
            TextBoxCorner.Parent = TextBox

            WriteBox.Name = "WriteBox"
            WriteBox.Parent = TextBox
            WriteBox.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            WriteBox.BackgroundTransparency = 1.000
            WriteBox.Position = UDim2.new(0.566666663, 0, 0.238889053, 0)
            WriteBox.Size = UDim2.new(0, 200, 0, 30)
            WriteBox.Font = Enum.Font.Gotham
            WriteBox.PlaceholderText = Display
            WriteBox.Text = ""
            WriteBox.TextColor3 = Color3.fromRGB(255, 255, 255)
            WriteBox.TextSize = 14.000
            WriteBox.TextXAlignment = Enum.TextXAlignment.Left

            WriteBoxUnderFrame.Name = "WriteBoxUnderFrame"
            WriteBoxUnderFrame.Parent = WriteBox
            WriteBoxUnderFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            WriteBoxUnderFrame.BorderSizePixel = 0
            WriteBoxUnderFrame.Position = UDim2.new(-0.0299999993, 0, 0.966666639, 0)
            WriteBoxUnderFrame.Size = UDim2.new(0, 206, 0, 1)

            text_format.Name = "text_format"
            text_format.Parent = TextBox
            text_format.BackgroundTransparency = 1.000
            text_format.LayoutOrder = 7
            text_format.Position = UDim2.new(0.935000002, 0, 0.316666663, 0)
            text_format.Size = UDim2.new(0, 25, 0, 25)
            text_format.ZIndex = 2
            text_format.Image = "rbxassetid://3926305904"
            text_format.ImageRectOffset = Vector2.new(804, 804)
            text_format.ImageRectSize = Vector2.new(36, 36)

            TextBox.MouseEnter:Connect(function()
                ts:Create(TextBox, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 0}):Play()
            end)
            TextBox.MouseLeave:Connect(function()
                ts:Create(TextBox, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 1}):Play()
            end)
            WriteBox.Focused:Connect(function()
                ts:Create(WriteBoxUnderFrame, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 0}):Play()
            end)
            WriteBox.FocusLost:Connect(function()
                ts:Create(WriteBoxUnderFrame, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = .75}):Play()
            end)

            WriteBox.FocusLost:Connect(function(enter)
                if enter then
                    if table.find(Index, WriteBox.Text) then
                        if #WriteBox.Text > 0 then
                            pcall(callback, WriteBox.Text)
                            ts:Create(WriteBoxUnderFrame, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 0}):Play()
                            ts:Create(WriteBoxUnderFrame, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundColor3 = Color3.fromRGB(102, 255, 0)}):Play()
                            wait(1)
                            ts:Create(WriteBoxUnderFrame, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundColor3 = Color3.fromRGB(255, 255, 255)}):Play()
                            ts:Create(WriteBoxUnderFrame, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = .75}):Play()
                            ts:Create(TextBox, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 1}):Play()
                        end
                    else
                        if #WriteBox.Text > 0 then
                            local goback = WriteBox.Text
                            ts:Create(WriteBoxUnderFrame, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 0}):Play()
                            ts:Create(WriteBoxUnderFrame, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundColor3 = Color3.fromRGB(255, 0, 4)}):Play()
                            WriteBox.PlaceholderText = "Not Found."
                            WriteBox.Text = "Not Found."
                            wait(1)
                            WriteBox.Text = goback
                            ts:Create(WriteBoxUnderFrame, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundColor3 = Color3.fromRGB(255, 255, 255)}):Play()
                            ts:Create(TextBox, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 1}):Play()
                            WriteBox.PlaceholderText = "..."
                        end
                    end
                end
            end)
            CurrentTab.CanvasSize = UDim2.new(0,0,0,List.AbsoluteContentSize.Y)
        end
        function TabContent:Slider(SldrTitle, SldrDesc, min, max, callback)
            min = min or 0
            max = max or 100
            local mouse = plr:GetMouse()
            local uis = game:service'UserInputService'
            local Value;
            local Slider = Instance.new("Frame")
            local SliderCorner = Instance.new("UICorner")
            local SliderTitle = Instance.new("TextLabel")
            local SliderDesc = Instance.new("TextLabel")
            local gesture = Instance.new("ImageButton")
            local SliderOutter = Instance.new("ImageButton")
            local SliderOutterCorner = Instance.new("UICorner")
            local Inner = Instance.new("ImageLabel")
            local InnerCorner = Instance.new("UICorner")
            local PercentageCounter = Instance.new("TextLabel")

            --Properties:

            Slider.Name = SldrTitle.."Slider"
            Slider.Parent = CurrentTab
            Slider.BackgroundTransparency = 1.000
            Slider.BackgroundColor3 = Color3.fromRGB(26, 26, 26)
            Slider.Position = UDim2.new(0.0161290169, 0, 0.346666694, 0)
            Slider.Size = UDim2.new(0, 600, 0, 60)

            SliderCorner.CornerRadius = UDim.new(0, 4)
            SliderCorner.Name = "SliderCorner"
            SliderCorner.Parent = Slider

            SliderTitle.Name = "SliderTitle"
            SliderTitle.Parent = Slider
            SliderTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            SliderTitle.BackgroundTransparency = 1.000
            SliderTitle.Position = UDim2.new(0.0333333313, 0, 0.106666565, 0)
            SliderTitle.Size = UDim2.new(0, 196, 0, 25)
            SliderTitle.Font = Enum.Font.Gotham
            SliderTitle.Text = SldrTitle
            SliderTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
            SliderTitle.TextSize = 14.000
            SliderTitle.TextXAlignment = Enum.TextXAlignment.Left

            SliderDesc.Name = "SliderDesc"
            SliderDesc.Parent = SliderTitle
            SliderDesc.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            SliderDesc.BackgroundTransparency = 1.000
            SliderDesc.Position = UDim2.new(0, 0, 0.746666551, 0)
            SliderDesc.Size = UDim2.new(0, 196, 0, 25)
            SliderDesc.Font = Enum.Font.Gotham
            SliderDesc.Text = SldrDesc
            SliderDesc.TextColor3 = Color3.fromRGB(255, 255, 255)
            SliderDesc.TextSize = 12.000
            SliderDesc.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
            SliderDesc.TextXAlignment = Enum.TextXAlignment.Left

            gesture.Name = "gesture"
            gesture.Parent = Slider
            gesture.BackgroundTransparency = 1.000
            gesture.LayoutOrder = 5
            gesture.Position = UDim2.new(0.935000002, 0, 0.283333331, 0)
            gesture.Size = UDim2.new(0, 25, 0, 25)
            gesture.ZIndex = 2
            gesture.Image = "rbxassetid://3926305904"
            gesture.ImageRectOffset = Vector2.new(4, 84)
            gesture.ImageRectSize = Vector2.new(36, 36)

            SliderOutter.Name = "SliderOutter"
            SliderOutter.Parent = Slider
            SliderOutter.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
            SliderOutter.BorderSizePixel = 0
            SliderOutter.Position = UDim2.new(0.495094329, 0, 0.42200014, 0)
            SliderOutter.Size = UDim2.new(0, 242, 0, 8) 
            SliderOutter.Image = ""

            SliderOutterCorner.Name = "SliderOutterCorner"
            SliderOutterCorner.Parent = SliderOutter

            Inner.Name = "Inner"
            Inner.Parent = SliderOutter
            Inner.BackgroundColor3 = Color3.fromRGB(79, 5, 111)
            Inner.Size = UDim2.new(0, 19, 0, 8)
            Inner.Image = ""
            Inner.ZIndex = 2

            InnerCorner.Name = "InnerCorner"
            InnerCorner.Parent = Inner

            PercentageCounter.Name = "PercentageCounter"
            PercentageCounter.Parent = Slider
            PercentageCounter.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            PercentageCounter.BackgroundTransparency = 1.000
            PercentageCounter.Position = UDim2.new(0.694999993, 0, 0.100000001, 0)
            PercentageCounter.Size = UDim2.new(0, 130, 0, 11)
            PercentageCounter.Font = Enum.Font.Gotham
            PercentageCounter.Text = "0"
            PercentageCounter.TextColor3 = Color3.fromRGB(255, 255, 255)
            PercentageCounter.TextSize = 14.000

            Slider.MouseEnter:Connect(function()
                ts:Create(Slider, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 0}):Play()
            end)
            Slider.MouseLeave:Connect(function()
                ts:Create(Slider, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 1}):Play()
            end)
            SliderOutter.MouseButton1Down:Connect(function()
                Value = math.floor((((tonumber(max) - tonumber(min)) / 242) * Inner.AbsoluteSize.X) + tonumber(min)) or 0
                pcall(function()
                    callback(Value)
                end)
                Inner.Size = UDim2.new(0, math.clamp(mouse.X - Inner.AbsolutePosition.X, 0, 242), 0, 8)
                moveconnection = mouse.Move:Connect(function()
                    PercentageCounter.Text = Value
                    Value = math.floor((((tonumber(max) - tonumber(min)) / 242) * Inner.AbsoluteSize.X) + tonumber(min))
                    Inner.Size = UDim2.new(0, math.clamp(mouse.X - Inner.AbsolutePosition.X, 0, 242), 0, 8)
                end)
                releaseconnection = uis.InputEnded:Connect(function(Mouse)
                    if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then
                        Value = math.floor((((tonumber(max) - tonumber(min)) / 242) * Inner.AbsoluteSize.X) + tonumber(min))
                        pcall(function()
                            callback(Value)
                        end)
                        Inner.Size = UDim2.new(0, math.clamp(mouse.X - Inner.AbsolutePosition.X, 0, 242), 0, 8)
                        moveconnection:Disconnect()
                        releaseconnection:Disconnect()
                    end
                end)
            end)
            CurrentTab.CanvasSize = UDim2.new(0,0,0,List.AbsoluteContentSize.Y)
        end
        function TabContent:DropDown(DrpTitle, DrpDesc, DisplayThing, list, callback)
            local itemcount = 0
            local framesize = 0
            local Dropped = false
            local Show = DisplayThing or "..."
            local callback = callback or function() end
            local Dropdown = Instance.new("Frame")
            local DropdownCorner = Instance.new("UICorner")
            local DropdownTitle = Instance.new("TextLabel")
            local DropdownDesc = Instance.new("TextLabel")
            local sortImage = Instance.new("ImageLabel")
            local DropdownButton = Instance.new("TextButton")
            local DropdownButtonCorner = Instance.new("UICorner")
            local DropdownButtonTitle = Instance.new("TextLabel")
            local DropdownButtonUnderFrame = Instance.new("Frame")
            local DropdownFrame = Instance.new("ScrollingFrame")
            local DropdownFrameListLayout = Instance.new("UIListLayout")
            
            --Properties:

            Dropdown.Name = "Dropdown"
            Dropdown.Parent = CurrentTab
            Dropdown.BackgroundTransparency = 1.000
            Dropdown.BackgroundColor3 = Color3.fromRGB(26, 26, 26)
            Dropdown.Position = UDim2.new(0.0161290169, 0, 0.664444447, 0)
            Dropdown.Size = UDim2.new(0, 600, 0, 60)

            DropdownCorner.CornerRadius = UDim.new(0, 4)
            DropdownCorner.Name = "DropdownCorner"
            DropdownCorner.Parent = Dropdown

            DropdownTitle.Name = "DropdownTitle"
            DropdownTitle.Parent = Dropdown
            DropdownTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            DropdownTitle.BackgroundTransparency = 1.000
            DropdownTitle.Position = UDim2.new(0.0333333313, 0, 0.106666565, 0)
            DropdownTitle.Size = UDim2.new(0, 196, 0, 25)
            DropdownTitle.Font = Enum.Font.Gotham
            DropdownTitle.Text = DrpTitle
            DropdownTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
            DropdownTitle.TextSize = 14.000
            DropdownTitle.TextXAlignment = Enum.TextXAlignment.Left

            DropdownDesc.Name = "DropdownDesc"
            DropdownDesc.Parent = DropdownTitle
            DropdownDesc.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            DropdownDesc.BackgroundTransparency = 1.000
            DropdownDesc.Position = UDim2.new(0, 0, 0.746666551, 0)
            DropdownDesc.Size = UDim2.new(0, 196, 0, 25)
            DropdownDesc.Font = Enum.Font.Gotham
            DropdownDesc.Text = DrpDesc
            DropdownDesc.TextColor3 = Color3.fromRGB(255, 255, 255)
            DropdownDesc.TextSize = 12.000
            DropdownDesc.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
            DropdownDesc.TextXAlignment = Enum.TextXAlignment.Left

            sortImage.Name = "sort"
            sortImage.Parent = Dropdown
            sortImage.BackgroundTransparency = 1.000
            sortImage.Position = UDim2.new(0.935000002, 0, 0.283333331, 0)
            sortImage.Size = UDim2.new(0, 25, 0, 25)
            sortImage.ZIndex = 2
            sortImage.Image = "rbxassetid://3926307971"
            sortImage.ImageRectOffset = Vector2.new(404, 44)
            sortImage.ImageRectSize = Vector2.new(36, 36)

            DropdownButton.Name = "DropdownButton"
            DropdownButton.Parent = Dropdown
            DropdownButton.BackgroundColor3 = Color3.fromRGB(33, 33, 33)
            DropdownButton.BackgroundTransparency = 1.000
            DropdownButton.BorderSizePixel = 0
            DropdownButton.Position = UDim2.new(0.433427721, 0, 0.267778516, 0)
            DropdownButton.Size = UDim2.new(0, 250, 0, 25)
            DropdownButton.Font = Enum.Font.Gotham
            DropdownButton.Text = ""
            DropdownButton.TextColor3 = Color3.fromRGB(255, 255, 255)
            DropdownButton.TextSize = 14.000

            DropdownButtonCorner.CornerRadius = UDim.new(0, 5)
            DropdownButtonCorner.Name = "DropdownButtonCorner"
            DropdownButtonCorner.Parent = DropdownButton

            DropdownButtonTitle.Name = "DropdownButtonTitle"
            DropdownButtonTitle.Parent = DropdownButton
            DropdownButtonTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            DropdownButtonTitle.BackgroundTransparency = 1.000
            DropdownButtonTitle.Position = UDim2.new(0.128000006, 0, 0, 0)
            DropdownButtonTitle.Size = UDim2.new(0, 200, 0, 25)
            DropdownButtonTitle.Font = Enum.Font.Gotham
            DropdownButtonTitle.Text = Show
            DropdownButtonTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
            DropdownButtonTitle.TextSize = 14.000
            DropdownButtonTitle.TextXAlignment = Enum.TextXAlignment.Right

            DropdownButtonUnderFrame.Name = "DropdownButtonUnderFrame"
            DropdownButtonUnderFrame.Parent = DropdownButton
            DropdownButtonUnderFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            DropdownButtonUnderFrame.BorderSizePixel = 0
            DropdownButtonUnderFrame.Position = UDim2.new(-3.7252903e-09, 0, 0.959999979, 0)
            DropdownButtonUnderFrame.Size = UDim2.new(0, 242, 0, 1)

            DropdownFrame.Name = "DropdownFrame"
            DropdownFrame.Parent = Dropdown
            DropdownFrame.Active = true
            DropdownFrame.Visible = false
            DropdownFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            DropdownFrame.BackgroundTransparency = 1.000
            DropdownFrame.BorderSizePixel = 0
            DropdownFrame.Position = UDim2.new(0.043333333, 0, 0.351724148, 0)
            DropdownFrame.Size = UDim2.new(0, 490, 0, 90)
            DropdownFrame.ScrollBarThickness = 0

            DropdownFrameListLayout.Parent = DropdownFrame
            DropdownFrameListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Right
            DropdownFrameListLayout.SortOrder = Enum.SortOrder.LayoutOrder
            DropdownFrameListLayout.Padding = UDim.new(0, 5)

            Dropdown.MouseEnter:Connect(function()
                ts:Create(Dropdown, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 0}):Play()
            end)
            Dropdown.MouseLeave:Connect(function()
                ts:Create(Dropdown, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 1}):Play()
            end)

            DropdownButton.MouseButton1Click:Connect(function()
                if Dropped == false then
                    DropdownFrame.Visible = true
                    Dropdown.Size = UDim2.new(0, 600, 0, 145)
                    DropdownButton.Position = UDim2.new(0.443, 0,0.109, 0)
                    DropdownTitle.Position = UDim2.new(0.033, 0,0.051, 0)
                    CurrentTab.CanvasSize = UDim2.new(0,0,0,List.AbsoluteContentSize.Y)
                    sortImage.Position = UDim2.new(0.935, 0,0.104, 0)
                else
                    sortImage.Position = UDim2.new(0.935000002, 0, 0.283333331, 0)
                    Dropdown.Size = UDim2.new(0, 600, 0, 60)
                    DropdownButton.Position = UDim2.new(0.433427721, 0, 0.267778516, 0)
                    DropdownTitle.Position = UDim2.new(0.0333333313, 0, 0.106666565, 0)
                    DropdownFrame.Visible = false
                    ts:Create(Dropdown, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 1}):Play()
                    CurrentTab.CanvasSize = UDim2.new(0,0,0,List.AbsoluteContentSize.Y)
                end
                Dropped = not Dropped
            end)
            for i,v in next, list do
                itemcount = itemcount + 1
                
                if itemcount == 1 then
                    framesize = 29
                elseif itemcount == 2 then
                    framesize = 58
                elseif itemcount >= 3 then
                    framesize = 87
                end
                
                local Item = Instance.new("TextButton")
                local ItemCorner = Instance.new("UICorner")
                local ItemTitle = Instance.new("TextLabel")
    
                Item.Name = "Item"
                Item.Parent = DropdownFrame
                Item.BackgroundTransparency = 1.000
                Item.BackgroundColor3 = Color3.fromRGB(33, 33, 33)
                Item.Position = UDim2.new(0.175510257, 0, -2.96896553, 0)
                Item.Size = UDim2.new(0, 480, 0, 25)
                Item.Font = Enum.Font.SourceSans
                Item.Text = ""
                Item.TextColor3 = Color3.fromRGB(0, 0, 0)
                Item.TextSize = 14.000
                Item.AutoButtonColor = false
                
                ItemCorner.CornerRadius = UDim.new(0, 4)
                ItemCorner.Name = "ItemCorner"
                ItemCorner.Parent = Item
                
                ItemTitle.Name = "ItemTitle"
                ItemTitle.Parent = Item
                ItemTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
                ItemTitle.BackgroundTransparency = 1.000
                ItemTitle.Position = UDim2.new(0.556249976, 0, 0, 0)
                ItemTitle.Size = UDim2.new(0, 200, 0, 25)
                ItemTitle.Font = Enum.Font.Gotham
                ItemTitle.Text = v
                ItemTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
                ItemTitle.TextSize = 14.000
                ItemTitle.TextXAlignment = Enum.TextXAlignment.Right
                ItemTitle.TextTransparency = .5

                DropdownFrame.CanvasSize = UDim2.new(0,0,0,DropdownFrameListLayout.AbsoluteContentSize.Y)
                
                Item.MouseEnter:Connect(function()
                    ts:Create(Item, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 0}):Play()
                    ts:Create(ItemTitle, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{TextTransparency = 0}):Play()
                end)
                
                Item.MouseLeave:Connect(function()
                    ts:Create(Item, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 1}):Play()
                    ts:Create(ItemTitle, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{TextTransparency = 0.55}):Play()
                end)
                
                Item.MouseButton1Click:Connect(function()
                    DropdownButtonTitle.Text = v
                    pcall(callback, v)
                    sortImage.Position = UDim2.new(0.935000002, 0, 0.283333331, 0)
                    Dropdown.Size = UDim2.new(0, 600, 0, 60)
                    DropdownButton.Position = UDim2.new(0.433427721, 0, 0.267778516, 0)
                    DropdownTitle.Position = UDim2.new(0.0333333313, 0, 0.106666565, 0)
                    DropdownFrame.Visible = false
                    ts:Create(Dropdown, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 1}):Play()
                    CurrentTab.CanvasSize = UDim2.new(0,0,0,List.AbsoluteContentSize.Y)
                    Dropped = not Dropped
                end)
            end
            CurrentTab.CanvasSize = UDim2.new(0,0,0,List.AbsoluteContentSize.Y)
        end

        function TabContent:KeyBind(KeyBndTitle, KeybndDesc, Binding, callback)
            local callback = callback or function() end
            local Key = Binding.Name
            local KeyBind = Instance.new("Frame")
            local KeyBindTitle = Instance.new("TextLabel")
            local KeyBindDesc = Instance.new("TextLabel")
            local KeyBindCorner = Instance.new("UICorner")
            local explicit = Instance.new("ImageButton")
            local KeyBindLabel = Instance.new("TextButton")
            local KeyBindLabelUnderFrame = Instance.new("Frame")

            --Properties:

            KeyBind.Name = "KeyBind"
            KeyBind.Parent = CurrentTab
            KeyBind.BackgroundTransparency = 1.000
            KeyBind.BackgroundColor3 = Color3.fromRGB(26, 26, 26)
            KeyBind.Position = UDim2.new(0.0146370828, 0, -0.310444474, 0)
            KeyBind.Size = UDim2.new(0, 600, 0, 60)

            KeyBindTitle.Name = "KeyBindTitle"
            KeyBindTitle.Parent = KeyBind
            KeyBindTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            KeyBindTitle.BackgroundTransparency = 1.000
            KeyBindTitle.Position = UDim2.new(0.0333333313, 0, 0.106666565, 0)
            KeyBindTitle.Size = UDim2.new(0, 196, 0, 25)
            KeyBindTitle.Font = Enum.Font.Gotham
            KeyBindTitle.Text = KeyBndTitle
            KeyBindTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
            KeyBindTitle.TextSize = 14.000
            KeyBindTitle.TextXAlignment = Enum.TextXAlignment.Left

            KeyBindDesc.Name = "KeyBindDesc"
            KeyBindDesc.Parent = KeyBindTitle
            KeyBindDesc.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            KeyBindDesc.BackgroundTransparency = 1.000
            KeyBindDesc.Position = UDim2.new(0, 0, 0.746666551, 0)
            KeyBindDesc.Size = UDim2.new(0, 196, 0, 25)
            KeyBindDesc.Font = Enum.Font.Gotham
            KeyBindDesc.Text = KeybndDesc
            KeyBindDesc.TextColor3 = Color3.fromRGB(255, 255, 255)
            KeyBindDesc.TextSize = 12.000
            KeyBindDesc.TextStrokeColor3 = Color3.fromRGB(255, 255, 255)
            KeyBindDesc.TextXAlignment = Enum.TextXAlignment.Left

            KeyBindCorner.CornerRadius = UDim.new(0, 4)
            KeyBindCorner.Name = "KeyBindCorner"
            KeyBindCorner.Parent = KeyBind

            explicit.Name = "explicit"
            explicit.Parent = KeyBind
            explicit.BackgroundTransparency = 1.000
            explicit.Position = UDim2.new(0.935000002, 0, 0.283333331, 0)
            explicit.Size = UDim2.new(0, 25, 0, 25)
            explicit.ZIndex = 2
            explicit.Image = "rbxassetid://3926305904"
            explicit.ImageRectOffset = Vector2.new(364, 284)
            explicit.ImageRectSize = Vector2.new(36, 36)

            KeyBindLabel.Name = "KeyBindLabel"
            KeyBindLabel.Parent = KeyBind
            KeyBindLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            KeyBindLabel.BackgroundTransparency = 1.000
            KeyBindLabel.Position = UDim2.new(0.556666613, 0, 0.200000018, 0)
            KeyBindLabel.Size = UDim2.new(0, 200, 0, 30)
            KeyBindLabel.Font = Enum.Font.Gotham
            KeyBindLabel.Text = Binding.Name
            KeyBindLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            KeyBindLabel.TextSize = 18.000
            KeyBindLabel.TextXAlignment = Enum.TextXAlignment.Left

            KeyBindLabelUnderFrame.Name = "KeyBindLabelUnderFrame"
            KeyBindLabelUnderFrame.Parent = KeyBindLabel
            KeyBindLabelUnderFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            KeyBindLabelUnderFrame.BorderSizePixel = 0
            KeyBindLabelUnderFrame.Position = UDim2.new(-0.0149999997, 0, 0.966666639, 0)
            KeyBindLabelUnderFrame.Size = UDim2.new(0, 206, 0, 1)

            KeyBind.MouseEnter:Connect(function()
                ts:Create(KeyBind, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 0}):Play()
            end)
            KeyBind.MouseLeave:Connect(function()
                ts:Create(KeyBind, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 1}):Play()
            end)

            KeyBindLabel.MouseButton1Click:Connect(function()
                KeyBindLabel.Text = 'Insert Key...'
                local inputwait = game:service'UserInputService'.InputBegan:wait()
                if inputwait.KeyCode.Name ~= 'Unknown' then
                    KeyBindLabel.Text = inputwait.KeyCode.Name
                    Key = inputwait.KeyCode.Name
                end
                ts:Create(KeyBind, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),{BackgroundTransparency = 1}):Play()
            end)
            
            game:service'UserInputService'.InputBegan:connect(function(current, pressed)
                if not pressed then
                    if current.KeyCode.Name == Key then
                        pcall(callback)
                    end
                end
            end)
            CurrentTab.CanvasSize = UDim2.new(0,0,0,List.AbsoluteContentSize.Y)
        end
        return TabContent
    end
    return Content
end
return Lib
