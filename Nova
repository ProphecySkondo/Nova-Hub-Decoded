            ToggleFrame.ScaleType = Enum.ScaleType.Slice
            ToggleFrame.SliceCenter = Rect.new(100, 100, 100, 100)
            ToggleFrame.SliceScale = 0.120

            ToggleButton.Name = "ToggleButton"
