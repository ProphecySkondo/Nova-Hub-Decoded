        end
    end
    function Lib:Notification(NotifMsg)
        local NotifyFrame = Instance.new("Frame")
        local NotifyTitle = Instance.new("TextLabel")
        local NotifyCorner = Instance.new("UICorner")
        local notifications = Instance.new("ImageButton")
        local shadowHolder = Instance.new("Frame")
        local umbraShadow = Instance.new("ImageLabel")
        local penumbraShadow = Instance.new("ImageLabel")
        local ambientShadow = Instance.new("ImageLabel")

        --Properties:

        NotifyFrame.Name = "Notify"
        NotifyFrame.Parent = Holder
        NotifyFrame.BackgroundColor3 = Color3.fromRGB(26, 26, 26)
        NotifyFrame.Position = UDim2.new(0, -550,0, 900)
        NotifyFrame.Size = UDim2.new(0, 265, 0, 50)
        NotifyFrame.Visible = true

        NotifyTitle.Name = "NotifyTitle"
        NotifyTitle.Parent = NotifyFrame
        NotifyTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        NotifyTitle.BackgroundTransparency = 1.000
        NotifyTitle.Position = UDim2.new(0.0339622647, 0, 0.239999995, 0)
        NotifyTitle.Size = UDim2.new(0, 215, 0, 25)
        NotifyTitle.Font = Enum.Font.Gotham
        NotifyTitle.Text = NotifMsg
        NotifyTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
        NotifyTitle.TextSize = 14.000
        NotifyTitle.TextXAlignment = Enum.TextXAlignment.Left
        NotifyTitle.TextTransparency = 1.000

        NotifyCorner.CornerRadius = UDim.new(0, 4)
        NotifyCorner.Name = "NotifyCorner"
        NotifyCorner.Parent = NotifyFrame

        notifications.Name = "notifications"
        notifications.Parent = NotifyFrame
        notifications.BackgroundTransparency = 1.000
        notifications.Position = UDim2.new(0.869811296, 0, 0.24000001, 0)
        notifications.Size = UDim2.new(0, 25, 0, 25)
        notifications.ZIndex = 2
        notifications.Image = "rbxassetid://3926305904"
        notifications.ImageRectOffset = Vector2.new(844, 564)
        notifications.ImageRectSize = Vector2.new(36, 36)

        shadowHolder.Name = "shadowHolder"
        shadowHolder.Parent = NotifyFrame
        shadowHolder.BackgroundTransparency = 1.000
        shadowHolder.Size = UDim2.new(1, 0, 1, 0)

        umbraShadow.Name = "umbraShadow"
        umbraShadow.Parent = shadowHolder
        umbraShadow.AnchorPoint = Vector2.new(0.5, 0.5)
        umbraShadow.BackgroundTransparency = 1.000
        umbraShadow.Position = UDim2.new(0.5, 0, 0.5, 6)
        umbraShadow.Size = UDim2.new(1, 10, 1, 10)
        umbraShadow.Image = "rbxassetid://1316045217"
        umbraShadow.ImageColor3 = Color3.fromRGB(0, 0, 0)
        umbraShadow.ImageTransparency = 0.860
        umbraShadow.ScaleType = Enum.ScaleType.Slice
        umbraShadow.SliceCenter = Rect.new(10, 10, 118, 118)

        penumbraShadow.Name = "penumbraShadow"
        penumbraShadow.Parent = shadowHolder
        penumbraShadow.AnchorPoint = Vector2.new(0.5, 0.5)
        penumbraShadow.BackgroundTransparency = 1.000
        penumbraShadow.Position = UDim2.new(0.5, 0, 0.5, 6)
        penumbraShadow.Size = UDim2.new(1, 10, 1, 10)
        penumbraShadow.Image = "rbxassetid://1316045217"
        penumbraShadow.ImageColor3 = Color3.fromRGB(0, 0, 0)
        penumbraShadow.ImageTransparency = 0.880
        penumbraShadow.ScaleType = Enum.ScaleType.Slice
        penumbraShadow.SliceCenter = Rect.new(10, 10, 118, 118)

        ambientShadow.Name = "ambientShadow"
        ambientShadow.Parent = shadowHolder
        ambientShadow.AnchorPoint = Vector2.new(0.5, 0.5)
        ambientShadow.BackgroundTransparency = 1.000
        ambientShadow.Position = UDim2.new(0.5, 0, 0.5, 6)
        ambientShadow.Size = UDim2.new(1, 10, 1, 10)
        ambientShadow.Image = "rbxassetid://1316045217"
        ambientShadow.ImageColor3 = Color3.fromRGB(0, 0, 0)
        ambientShadow.ImageTransparency = 0.880
        ambientShadow.ScaleType = Enum.ScaleType.Slice
        ambientShadow.SliceCenter = Rect.new(10, 10, 118, 118)

        ts:create(NotifyFrame, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {Position = UDim2.new(0, 20,0, 900)}):Play()
        ts:create(NotifyTitle, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 0}):Play()
        wait(2)
        ts:create(NotifyTitle, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {TextTransparency = 1}):Play()
        ts:create(NotifyFrame, TweenInfo.new(.3, Enum.EasingStyle.Quint, Enum.EasingDirection.Out), {Position = UDim2.new(0, -550,0, 900)}):Play()
        wait(.7)
        NotifyFrame:Destroy()
    end
    function Lib:InternalNotification(Type, NotifDesc)
        local TitleTxt
        local Img
        local Pos
        
        if Type == "Good" then
            Img = "http://www.roblox.com/asset/?id=8589545938"
            TitleTxt = "Success!"
            Pos = UDim2.new(0.092, 0,0.067, 0)
        elseif Type == "Warning" then
            Img = "http://www.roblox.com/asset/?id=1764960410"
            TitleTxt = "Warning!"
            Pos = UDim2.new(0.0833333358, 0, 0.00833333284, 0)
        elseif Type == "Bad" then
            Img = "http://www.roblox.com/asset/?id=4595009422"
            TitleTxt = "Uh Oh, something went wrong"
            Pos = UDim2.new(0.092, 0,0.067, 0)
        end

        local AppNotification = Instance.new("Frame")
        local NotificationFrame = Instance.new("Frame")
        local NotificationFrameCorner = Instance.new("UICorner")
        local NotificationTitle = Instance.new("TextLabel")
        local NotificationDesc = Instance.new("TextLabel")
        local NotificationPictureFrame = Instance.new("Frame")
        local NotificatioPictureFrameCorner = Instance.new("UICorner")
        local NotifcationImage = Instance.new("ImageLabel")
        local ReactButton = Instance.new("TextButton")
        local ReactButtonCorner = Instance.new("UICorner")
        local ReactButtonHoverSound = Instance.new("Sound", ReactButton)
        local ReactButtonClickSound = Instance.new("Sound", ReactButton)

        ReactButtonHoverSound.Name = "HoverSound"
        ReactButtonHoverSound.SoundId = "rbxassetid://1238528678"
        ReactButtonHoverSound.Volume = 1
        
        ReactButtonClickSound.Name = "ClickSound"
        ReactButtonClickSound.SoundId = "rbxassetid://177266782"
        ReactButtonClickSound.Volume = 1

        --Properties:

        AppNotification.Name = "AppNotification"
        AppNotification.Parent = Main
        AppNotification.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
        AppNotification.BackgroundTransparency = 0.500
        AppNotification.Size = UDim2.new(0, 800, 0, 500)

        NotificationFrame.Name = "NotificationFrame"
        NotificationFrame.Parent = AppNotification
        NotificationFrame.BackgroundColor3 = Color3.fromRGB(26, 26, 26)
        NotificationFrame.BorderSizePixel = 0
        NotificationFrame.Position = UDim2.new(0.25, 0, 0.200000003, 0)
        NotificationFrame.Size = UDim2.new(0, 400, 0, 300)

        NotificationFrameCorner.CornerRadius = UDim.new(0, 4)
        NotificationFrameCorner.Name = "NotificationFrameCorner"
        NotificationFrameCorner.Parent = NotificationFrame

        NotificationTitle.Name = "NotificationTitle"
        NotificationTitle.Parent = NotificationFrame
        NotificationTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        NotificationTitle.BackgroundTransparency = 1.000
        NotificationTitle.Position = UDim2.new(0.25, 0, 0.469999999, 0)
        NotificationTitle.Size = UDim2.new(0, 200, 0, 25)
        NotificationTitle.Font = Enum.Font.Gotham
        NotificationTitle.Text = TitleTxt
        NotificationTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
        NotificationTitle.TextScaled = true
        NotificationTitle.TextSize = 15.000
        NotificationTitle.TextWrapped = true

        NotificationDesc.Name = "NotificationDesc"
        NotificationDesc.Parent = NotificationTitle
        NotificationDesc.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        NotificationDesc.BackgroundTransparency = 1.000
        NotificationDesc.Position = UDim2.new(-0.164999992, 0, 0.799999952, 0)
        NotificationDesc.Size = UDim2.new(0, 265, 0, 53)
        NotificationDesc.Font = Enum.Font.Gotham
        NotificationDesc.Text = NotifDesc
        NotificationDesc.TextColor3 = Color3.fromRGB(255, 255, 255)
        NotificationDesc.TextSize = 14.000
        NotificationDesc.TextWrapped = true

        NotificationPictureFrame.Name = "NotificationPictureFrame"
        NotificationPictureFrame.Parent = NotificationFrame
        NotificationPictureFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
        NotificationPictureFrame.Position = UDim2.new(0.347499996, 0, 0.0399999991, 0)
        NotificationPictureFrame.Size = UDim2.new(0, 120, 0, 120)

        NotificatioPictureFrameCorner.CornerRadius = UDim.new(0, 90)
        NotificatioPictureFrameCorner.Name = "NotificatioPictureFrameCorner"
        NotificatioPictureFrameCorner.Parent = NotificationPictureFrame

        NotifcationImage.Name = "NotifcationImage"
        NotifcationImage.Parent = NotificationPictureFrame
        NotifcationImage.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        NotifcationImage.BackgroundTransparency = 1.000
        NotifcationImage.Position = Pos
        NotifcationImage.Size = UDim2.new(0, 100, 0, 100)
        NotifcationImage.Image = Img
        NotifcationImage.ImageTransparency = 0.100

        ReactButton.Name = "ReactButton"
        ReactButton.Parent = NotificationFrame
        ReactButton.BackgroundColor3 = Color3.fromRGB(78, 0, 121)
        ReactButton.BorderSizePixel = 0
        ReactButton.Position = UDim2.new(0.292499989, 0, 0.74333334, 0)
        ReactButton.Size = UDim2.new(0, 165, 0, 33)
        ReactButton.Font = Enum.Font.Gotham
        ReactButton.Text = "Okay!"
        ReactButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        ReactButton.TextSize = 18.000

        ReactButtonCorner.CornerRadius = UDim.new(0, 4)
        ReactButtonCorner.Name = "ReactButtonCorner"
        ReactButtonCorner.Parent = ReactButton

        ReactButton.MouseButton1Click:connect(function()
            AppNotification:Destroy()
        end)
    end

    for _, v in pairs(plr:GetFriendsOnline()) do
        local FriendForm = Instance.new("Frame")
        local FriendFormCorner = Instance.new("UICorner")
        local FriendFromGradient = Instance.new("UIGradient")
        local FriendName = Instance.new("TextLabel")
        local FriendDisplayName = Instance.new("TextLabel")
        local FriendStatus = Instance.new("TextLabel")
        local TeleportButton = Instance.new("TextButton")
        local TeleportButtonCorner = Instance.new("UICorner")
        local FriendGame = Instance.new("TextLabel")
        local IntId

        if v.PlaceId == nil then
            IntId = "Not Displayed"
        else
            IntId = game:GetService("MarketplaceService"):GetProductInfo(v.PlaceId).Name
        end
            
        --Properties:

        FriendForm.Name = "FriendForm"
        FriendForm.Parent = FriendFormScrollingFrame
        FriendForm.BackgroundColor3 = Color3.fromRGB(29, 29, 29)
        FriendForm.Position = UDim2.new(0.0790322497, 0, 0.582000017, 0)
        FriendForm.Size = UDim2.new(0, 160, 0, 117)

        FriendFormCorner.Name = "FriendFormCorner"
        FriendFormCorner.Parent = FriendForm

        FriendFromGradient.Color = ColorSequence.new{ColorSequenceKeypoint.new(0.00, Color3.fromRGB(255, 255, 255)), ColorSequenceKeypoint.new(1.00, Color3.fromRGB(0, 255, 0))}
        FriendFromGradient.Rotation = 45
        FriendFromGradient.Name = "FriendFromGradient"
        FriendFromGradient.Parent = FriendForm

        FriendName.Name = "FriendName"
        FriendName.Parent = FriendForm
        FriendName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        FriendName.BackgroundTransparency = 1.000
        FriendName.Position = UDim2.new(0.025000006, 0, -0.0073399432, 0)
        FriendName.Size = UDim2.new(0, 75, 0, 31)
        FriendName.Font = Enum.Font.GothamBold
