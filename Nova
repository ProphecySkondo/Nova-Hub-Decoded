    if table.find(Storage.FinalStandIds, game.PlaceId) then
        function Lib:StatCheckFS(Target)
            for i, v in pairs(workspace.Live:GetChildren()) do
                if string.match(Target, v.Name) then
                    for m , x in pairs(v:GetChildren()) do
                        if x.ClassName == "Model" then
                            if string.find(x.Name, "Prestige: ") then
                                local prespp = string.sub(x.Name, 18, 1000)
                                local noprespp = string.sub(x.Name, 5, 1000)
                                local StatFrame = Instance.new("Frame")
                                local StatFrameCorner = Instance.new("UICorner")
                                local StatFrameTitle = Instance.new("TextLabel")
                                local shadowHolder = Instance.new("Frame")
                                local umbraShadow = Instance.new("ImageLabel")
                                local penumbraShadow = Instance.new("ImageLabel")
                                local ambientShadow = Instance.new("ImageLabel")
                                local InfoHolder = Instance.new("Frame")
                                local InfoHolderLayOut = Instance.new("UIListLayout")
                                local NameLabel = Instance.new("TextLabel")
                                local LevelLabel = Instance.new("TextLabel")
                                local HealthLabel = Instance.new("TextLabel")
                                local KiMaxLabel = Instance.new("TextLabel")
                                local MeleeDamageLabel = Instance.new("TextLabel")
                                local KiDamageLabel = Instance.new("TextLabel")
                                local MeleeResLabel = Instance.new("TextLabel")
                                local KiResLabel = Instance.new("TextLabel")
                                local Speedabel = Instance.new("TextLabel")
                                local call_merge = Instance.new("ImageButton")
                                local StatFrameGradient = Instance.new("UIGradient")
                                local Title = Instance.new("ImageLabel")
                                
                                --Properties:
                                
