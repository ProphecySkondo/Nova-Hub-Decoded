--Nova Lib(Final)
repeat
    wait()
until game.Players.LocalPlayer and game.Players.LocalPlayer.Character and game:IsLoaded()

wait(1)
local Lib = {}
Run = game:service'RunService'
Int = game:service'TeleportService'
ts = game:service'TweenService'
http = game:service'HttpService'
UIS = game:service'UserInputService'
plr = game.Players.LocalPlayer
Char = plr.Character
