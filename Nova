    local Content = {}

    function Content:Tab(TabTitle, Icon)
        local Img
		local Offset1
		local Offset2
		if Icon == "User" then
			Img = "rbxassetid://3926307971"
			Offset1 = Vector2.new(884, 4)
			Offset2 = Vector2.new(36, 36)
        end
		if Icon == "Eye" then
			Img = "rbxassetid://3926307971"
			Offset1 = Vector2.new(44, 444)
			Offset2 = Vector2.new(36, 36)
        end
		if Icon == "Planet" then
			Img = "rbxassetid://3926305904"
			Offset1 = Vector2.new(604, 324)
			Offset2 = Vector2.new(36, 36)
        end
		if Icon == "Extra" then
			Img = "rbxassetid://3926307971"
			Offset1 = Vector2.new(404, 204)
			Offset2 = Vector2.new(36, 36)
        end
		if Icon == "Money" then
			Img = "rbxassetid://3926307971"
			Offset1 = Vector2.new(964, 484)
			Offset2 = Vector2.new(36, 36)
        end
        if Icon == "Auto" then
            Img = "rbxassetid://3926307971"
			Offset1 = Vector2.new(564, 444)
			Offset2 = Vector2.new(36, 36)
        end
        if Icon == "Upgrade" then
            Img = "rbxassetid://3926307971"
			Offset1 = Vector2.new(884, 164)
			Offset2 = Vector2.new(36, 36)
		end
        if Icon == "Star" then
            Img = "rbxassetid://3926305904"
			Offset1 = Vector2.new(116, 4)
			Offset2 = Vector2.new(24, 24)
        end
        local TabButton = Instance.new("TextButton")
        local TabCorner = Instance.new("UICorner")
        local person = Instance.new("ImageButton")
        local CurrentTab = Instance.new('ScrollingFrame') 
		local List = Instance.new('UIListLayout')
        local HoverSound = Instance.new("Sound", TabButton)
        local ClickSound = Instance.new("Sound", TabButton)

        TabButton.Name = TabTitle.."TabButton"
        TabButton.Parent = TabSection
        TabButton.BackgroundColor3 = Color3.fromRGB(78, 0, 121)
        TabButton.BackgroundTransparency = 1.000
        TabButton.BorderSizePixel = 0
        TabButton.Position = UDim2.new(0.0500000007, 0, 0, 0)
        TabButton.Size = UDim2.new(0, 135, 0, 35)
        TabButton.Font = Enum.Font.Gotham
        TabButton.Text = TabTitle
        TabButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        TabButton.TextSize = 14.000

        TabCorner.Name = "TabCorner"
        TabCorner.Parent = TabButton
        TabCorner.CornerRadius = UDim.new(0, 4)

        person.Name = "person"
        person.Parent = TabButton
        person.BackgroundTransparency = 1.000
        person.LayoutOrder = 6
        person.Position = UDim2.new(0.092592597, 0, 0.0666666627, 0)
        person.Size = UDim2.new(0, 25, 0, 25)
        person.ZIndex = 2
        person.Image = Img
        person.ImageRectOffset = Offset1
        person.ImageRectSize = Offset2

        CurrentTab.Name = TabTitle..'Tab'
		CurrentTab.Parent = TabsFolder
		CurrentTab.Active = true
		CurrentTab.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		CurrentTab.BackgroundTransparency = 1.000
		CurrentTab.Size = UDim2.new(0, 620, 0, 450)
		CurrentTab.ScrollBarThickness = 6
        CurrentTab.BorderSizePixel = 0
        CurrentTab.Visible = false
        CurrentTab.ClipsDescendants = true

		List.Name = 'List'
		List.Parent = CurrentTab
		List.HorizontalAlignment = Enum.HorizontalAlignment.Center
		List.SortOrder = Enum.SortOrder.LayoutOrder
		List.Padding = UDim.new(0, 5)

        HoverSound.Name = "HoverSound"
        HoverSound.SoundId = "rbxassetid://1238528678"
        HoverSound.Volume = 1
        
        ClickSound.Name = "ClickSound"
        ClickSound.SoundId = "rbxassetid://177266782"
        ClickSound.Volume = 1

        TabButton.MouseEnter:connect(function()
            if currentopentab ~= TabButton.Name then
                HoverSound:Play()
                ts:Create(TabButton, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundTransparency = 0}):Play()
            end
        end)
        TabButton.MouseLeave:connect(function()
            if currentopentab ~= TabButton.Name then
                ts:Create(TabButton, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
            end
        end)
        TabButton.MouseButton1Click:connect(function()
            ClickSound:Play()
            for i, v in next, TabSection:GetChildren() do
                if v.ClassName == "TextButton" then
                    ts:Create(v, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
                    ts:Create(HomeTabButton, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
                end
                ts:Create(TabButton, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundTransparency = 0}):Play()
            end
            currentopentab = TabButton.Name
            for i, v in next, TabsFolder:GetChildren() do
                v.Visible = false
                HomeTab.Visible = false
            end
            CurrentTab.Visible = true
        end)

        local TabContent = {}
        
        function TabContent:Section(SctnTitle)
            local Section = Instance.new("Frame")
            local SectionTitle = Instance.new("TextLabel")
            local SectionCorner = Instance.new("UICorner")

            --Properties:

            Section.Name = "Section"
            Section.Parent = CurrentTab
            Section.BackgroundTransparency = 1.000
            Section.BackgroundColor3 = Color3.fromRGB(26, 26, 26)
            Section.Position = UDim2.new(0.0161290318, 0, 1, 0)
            Section.Size = UDim2.new(0, 600, 0, 60)

            SectionTitle.Name = "SectionTitle"
            SectionTitle.Parent = Section
            SectionTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            SectionTitle.BackgroundTransparency = 1.000
            SectionTitle.BorderSizePixel = 0
            SectionTitle.Position = UDim2.new(0.0166666675, 0, 0, 0)
            SectionTitle.Size = UDim2.new(0, 575, 0, 60)
            SectionTitle.Font = Enum.Font.GothamSemibold
            SectionTitle.Text = SctnTitle
            SectionTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
            SectionTitle.TextSize = 22.000
            SectionTitle.TextXAlignment = Enum.TextXAlignment.Left

            SectionCorner.CornerRadius = UDim.new(0, 4)
            SectionCorner.Name = "SectionCorner"
            SectionCorner.Parent = Section
            
            CurrentTab.CanvasSize = UDim2.new(0,0,0,List.AbsoluteContentSize.Y)
        end
        
        function TabContent:Seperator()
            local Seperator = Instance.new("Frame")

            Seperator.Name = "Seperator"
            Seperator.Parent = CurrentTab
            Seperator.BackgroundColor3 = Color3.fromRGB(26, 26, 26)
            Seperator.BorderColor3 = Color3.fromRGB(27, 42, 53)
