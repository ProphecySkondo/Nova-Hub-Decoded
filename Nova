            Seperator.Position = UDim2.new(0.0161290318, 0, 1.15555561, 0)
            Seperator.Size = UDim2.new(0, 600, 0, 2)

            CurrentTab.CanvasSize = UDim2.new(0,0,0,List.AbsoluteContentSize.Y)
        end

        function TabContent:Button(BtnTitle, BtnDesc, callback)
            local callback = callback or function() end
            local Button = Instance.new("TextButton")
            local ButtonCorner = Instance.new("UICorner")
            local ButtonTitle = Instance.new("TextLabel")
            local ButtonDesc = Instance.new("TextLabel")
            local touch_app = Instance.new("ImageButton")
            local Sample = Instance.new("ImageLabel")

            --Properties:

            Button.Name = "Button"
            Button.Parent = CurrentTab
            Button.BackgroundColor3 = Color3.fromRGB(26, 26, 26)
