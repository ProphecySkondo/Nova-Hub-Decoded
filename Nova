function ToggleLib()
    if game.CoreGui.Holder.Main.Visible == false then
        game.CoreGui.Holder.Main.Visible = true
        game.CoreGui.Holder['NovaLibSound']:Play()
        game.Lighting['NovaBlur'].Enabled = true
        workspace.CurrentCamera.CameraType = Enum.CameraType.Scriptable
        Char.HumanoidRootPart.Anchored = true
    elseif game.CoreGui.Holder.Main.Visible == true then
        game.CoreGui.Holder.Main.Visible = false
        game.CoreGui.Holder.SettingMain.Visible = false
        game.CoreGui.Holder['NovaLibSound']:Stop()
        game.Lighting['NovaBlur'].Enabled = false
        workspace.CurrentCamera.CameraType = Enum.CameraType.Custom
        Char.HumanoidRootPart.Anchored = false
    end
end

