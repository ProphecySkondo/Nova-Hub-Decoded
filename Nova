        FriendDisplayName.Font = Enum.Font.Gotham
        FriendDisplayName.Text = v.DisplayName
        FriendDisplayName.TextColor3 = Color3.fromRGB(161, 161, 161)
        FriendDisplayName.TextSize = 10.000
        FriendDisplayName.TextXAlignment = Enum.TextXAlignment.Left

        FriendStatus.Name = "FriendStatus"
        FriendStatus.Parent = FriendForm
        FriendStatus.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        FriendStatus.BackgroundTransparency = 1.000
        FriendStatus.Position = UDim2.new(0.652884686, 0, -0.0100854076, 0)
        FriendStatus.Size = UDim2.new(0, 39, 0, 25)
        FriendStatus.Font = Enum.Font.Gotham
        FriendStatus.Text = "Online"
        FriendStatus.TextColor3 = Color3.fromRGB(255, 255, 255)
        FriendStatus.TextSize = 11.000
        FriendStatus.TextXAlignment = Enum.TextXAlignment.Left

        TeleportButton.Name = "TeleportButton"
        TeleportButton.Parent = FriendForm
        TeleportButton.BackgroundColor3 = Color3.fromRGB(0, 117, 56)
        TeleportButton.Position = UDim2.new(0.273076922, 0, 0.741452992, 0)
        TeleportButton.Size = UDim2.new(0, 72, 0, 20)
        TeleportButton.Font = Enum.Font.Gotham
        TeleportButton.Text = "Teleport"
        TeleportButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        TeleportButton.TextSize = 12.000

        TeleportButtonCorner.Name = "TeleportButtonCorner"
        TeleportButtonCorner.Parent = TeleportButton

        FriendGame.Name = "FriendGame"
        FriendGame.Parent = FriendForm
        FriendGame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
        FriendGame.BackgroundTransparency = 1.000
        FriendGame.Position = UDim2.new(0.0437499993, 0, 0.358974367, 0)
        FriendGame.Size = UDim2.new(0, 117, 0, 32)
        FriendGame.Font = Enum.Font.Gotham
        FriendGame.Text = "Game: "..IntId
        FriendGame.TextColor3 = Color3.fromRGB(255, 255, 255)
        FriendGame.TextSize = 13.000
        FriendGame.TextWrapped = true
        FriendGame.TextXAlignment = Enum.TextXAlignment.Left

        MenuSound.Name = "NovaLibSound"
        MenuSound.Parent = Holder
        MenuSound.SoundId = "rbxassetid://9046865270"
        MenuSound.Looped = true

        MenuBlur.Name = "NovaBlur"
        MenuBlur.Parent = game.Lighting
        MenuBlur.Enabled = false
        MenuBlur.Size = 1000

        TeleportButton.MouseButton1Click:connect(function()
            game:GetService("TeleportService"):TeleportToPlaceInstance(v.PlaceId, v.GameId)
        end)
        if game.Players:FindFirstChild(v.UserName) then
            FriendStatus.Text = "In Game"
        else
            FriendStatus.Text = "Online"
        end
    end

	ServerRegion.Text = "Xeno Don't Support Http So Yea"

	Run.Heartbeat:connect(function()
		local seconds = math.floor(workspace.DistributedGameTime)
        local minutes = math.floor(workspace.DistributedGameTime / 60)
        local hours = math.floor(workspace.DistributedGameTime / 60 / 60)
        local seconds = seconds - (minutes * 60)
        local minutes = minutes - (hours * 60)
        SessionTimeValue.Text = "" ..hours .. ":" .. minutes .. ":" .. seconds .. ""
        MonthDate.Text = os.date("%B %d, %Y")
        Weekday.Text = os.date("%A")
        Time.Text = os.date("%I:%M:%S %p")
		PlayerCount.Text = "Player Count: "..#game.Players:GetPlayers()
	end)

	game.Stats:WaitForChild("Network")
	game.Stats.Network:WaitForChild("ServerStatsItem")
	game.Stats.Network.ServerStatsItem:WaitForChild("Data Ping")

	Run.Heartbeat:Connect(function()
		ping = string.split(string.split(game.Stats.Network.ServerStatsItem["Data Ping"]:GetValueString(), " ")[1], ".")
		fps = string.split(game.Stats.Workspace.Heartbeat:GetValueString(), ".")
		memory = game:GetService'Stats':GetTotalMemoryUsageMb()
		gpu = string.split(string.split(game.Stats.PerformanceStats["GPU"]:GetValueString(), " ")[1], ".")
		cpu = string.split(string.split(game.Stats.PerformanceStats["CPU"]:GetValueString(), " ")[1], ".")
		FPSPerfomance.Text = " FPS: "..fps[1]
		PingPerfomance.Text = " Ping: "..ping[1]
		MemoryPerfomance.Text = " Memory Usage: "..string.sub(tostring(memory), 1, 4)
		GPUPerfomance.Text = " GPU Usage: "..gpu[1].." ms" 
		CPUPerfomance.Text = " CPU Usage: "..cpu[1].." ms"
        if MemoryPerfomance.Text == " Memory Usage:  1" then
            MemoryPerfomance.Text = " Memory Usage:  1 GB"
        end
	end)
    local HomeHoverSound = Instance.new("Sound", HomeTabButton)
    local HomeClickSound = Instance.new("Sound", HomeTabButton)

    HomeHoverSound.Name = "HoverSound"
    HomeHoverSound.SoundId = "rbxassetid://1238528678"
    HomeHoverSound.Volume = 1
    
    HomeClickSound.Name = "ClickSound"
    HomeClickSound.SoundId = "rbxassetid://177266782"
    HomeClickSound.Volume = 1

    HomeTabButton.MouseEnter:connect(function()
        if currentopentab ~= "Home" then
            HomeHoverSound:Play()
            ts:Create(HomeTabButton, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundTransparency = 0}):Play()
        end
    end)
    HomeTabButton.MouseLeave:connect(function()
        if currentopentab ~= "Home" then
            ts:Create(HomeTabButton, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
        end
    end)
    HomeTabButton.MouseButton1Click:connect(function()
        HomeClickSound:Play()
        for i, v in next, TabSection:GetChildren() do
            if v.ClassName == "TextButton" then
                ts:Create(v, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundTransparency = 1}):Play()
            end
            ts:Create(HomeTabButton, TweenInfo.new(.2, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {BackgroundTransparency = 0}):Play()
        end
        currentopentab = "Home"
        for i, v in next, TabsFolder:GetChildren() do
            v.Visible = false
        end 
        HomeTab.Visible = true
    end)
